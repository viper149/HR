                <div class="form-group">
                    <label asp-for="ComImpCnfinfo.CNFNAME" class="control-label"></label>
                    <input asp-for="ComImpCnfinfo.CNFNAME" class="form-control" />
                    <span asp-validation-for="ComImpCnfinfo.CNFNAME" class="text-danger"></span>
                </div>
            </div>
            <div class="col-lg-3 col-md-4 col-sm-6 col-xs-12">
                <div class="form-group">
                    <label asp-for="ComImpCnfinfo.ADDRESS" class="control-label"></label>
                    <input asp-for="ComImpCnfinfo.ADDRESS" class="form-control" />
                    <span asp-validation-for="ComImpCnfinfo.ADDRESS" class="text-danger"></span>
                </div>
            </div>
            <div class="col-lg-3 col-md-4 col-sm-6 col-xs-12">
                <div class="form-group">
                    <label asp-for="ComImpCnfinfo.C_PERSON" class="control-label"></label>
