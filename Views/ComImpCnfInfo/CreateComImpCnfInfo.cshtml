                    <label asp-for="ComImpCnfinfo.CELL_NO" class="control-label"></label>
                    <input asp-for="ComImpCnfinfo.CELL_NO" class="form-control" />
                    <span asp-validation-for="ComImpCnfinfo.CELL_NO" class="text-danger"></span>
                </div>
            </div>
            <div class="col-lg-3 col-md-4 col-sm-6 col-xs-12">
                <div class="form-group">
                    <label asp-for="ComImpCnfinfo.REMARKS" class="control-label"></label>
                    <input asp-for="ComImpCnfinfo.REMARKS" class="form-control" />
                    <span asp-validation-for="ComImpCnfinfo.REMARKS" class="text-danger"></span>
                </div>
            </div>

            <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                <div class="form-group pull-right">
                    <a asp-action="GetComImpCnfInfo" class="btn btn-dark" accesskey="b">Back to List</a>
                    <input type="submit" value="Create" class="btn btn-success" />
                </div>
            </div>
        </div>
    </form>
</div>

