
@model ComImpCnfInfoViewModel

@{
    ViewData["Title"] = "CNF Information";
}

<h2><i class="fa fa-info-circle text-success" aria-hidden="true"></i> Details @ViewData["Title"]</h2>
<hr />

@{
    if (Model != null)
    {
        <div class="row">
            <div class="col-md-12">
                <div class="row">
                    <div class="col-lg-12">
                        <div class="m-b-md">
                            <h2 class="text-danger">@Html.DisplayNameFor(e => e.ComImpCnfinfo.CNFNAME) : [ @Model.ComImpCnfinfo.CNFNAME ]</h2>
                            <hr />
                        </div>
                    </div>
                </div>
                <div class="row details">
                    <div class="col-2">
                        <div class="form-group">
                            <strong>
                                <label asp-for="ComImpCnfinfo.ADDRESS" class="control-label"></label>
                            </strong>
                            <span class="custom-form-control form-control bg-e9ecef">
                                @Html.DisplayFor(model => model.ComImpCnfinfo.ADDRESS)
                            </span>
                        </div>
                    </div>
                    <div class="col-2">
                        <div class="form-group">
                            <strong>
                                <label asp-for="ComImpCnfinfo.C_PERSON" class="control-label"></label>
                            </strong>
                            <span class="custom-form-control form-control bg-e9ecef">
                                @Html.DisplayFor(model => model.ComImpCnfinfo.C_PERSON)
                            </span>
                        </div>
                    </div>
                    <div class="col-2">
                        <div class="form-group">
                            <strong>
                                <label asp-for="ComImpCnfinfo.CELL_NO" class="control-label"></label>
                            </strong>
                            <span class="custom-form-control form-control bg-e9ecef">
                                @Html.DisplayFor(model => model.ComImpCnfinfo.CELL_NO)
                            </span>
                        </div>
                    </div>
                    <div class="col-2">
                        <div class="form-group">
                            <strong>
                                <label asp-for="ComImpCnfinfo.REMARKS" class="control-label"></label>
                            </strong>
                            <span class="custom-form-control form-control bg-e9ecef">
                                @Html.DisplayFor(model => model.ComImpCnfinfo.REMARKS)
                            </span>
                        </div>
                    </div>
                </div>
            </div>

            <div class="col-md-12">
                @Html.ActionLink("Edit", "EditComImpCnfInfo", new { cnfId = Model.ComImpCnfinfo.EncryptedId }, new { @class = "btn btn-warning pull-right" })
                <a class="btn btn-dark pull-right" asp-action="GetComImpCnfInfo" asp-controller="ComImpCnfInfo">Back to List</a>
            </div>
        </div>
    }
}