@using DevExpress.AspNetCore
@using DenimERP.Reports.Clearance
@using rptWashCodeWiseRollReport = DenimERP.Reports.Clearance.rptWashCodeWiseRollReport
@model int

@{
    ViewData["Title"] = "Wash Codewise Roll Report";
    Layout = Layout;
}
<h2>@ViewData["Title"]</h2>

@{

    @Html.DevExpress().WebDocumentViewer("WebDocumentViewer").Height("1000px").Bind(new rptWashCodeWiseRollReport(Model))
}

@section Styles {
    <partial name="_LayoutReport" />
}

@section Scripts {
    <script asp-append-version="true" src="~/js/jquery-resizable/jquery-resizable.js"></script>
}