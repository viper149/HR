@using DevExpress.AspNetCore
@using DenimERP.Reports.COM_EX
@using DenimERP.Reports.Finishing
@using rptShipmentFollowUp = DenimERP.Reports.COM_EX.rptShipmentFollowUp

@model int

@{
    ViewData["Title"] = "Shipment Date Follow Up List";
    Layout = Layout;
}
<h2>@ViewData["Title"]</h2>

@{

    @Html.DevExpress().WebDocumentViewer("WebDocumentViewer").Height("1000px").Bind(new rptShipmentFollowUp())
}

@section Styles {
    <partial name="_LayoutReport" />
}

@section Scripts {
    <script asp-append-version="true" src="~/js/jquery-resizable/jquery-resizable.js"></script>
}