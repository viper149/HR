@using DevExpress.AspNetCore
@using DenimERP.Reports.Finishing
@using rptDateWiseFinishingConsumption = DenimERP.Reports.Finishing.rptDateWiseFinishingConsumption

@model int

@{
    ViewData["Title"] = "Date Wise Finishing Consumption Report";
    Layout = Layout;
}
<h2>@ViewData["Title"]</h2>

@{

    @Html.DevExpress().WebDocumentViewer("WebDocumentViewer").Height("1000px").Bind(new rptDateWiseFinishingConsumption())
}

@section Styles {
    <partial name="_LayoutReport" />
}

@section Scripts {
    <script asp-append-version="true" src="~/js/jquery-resizable/jquery-resizable.js"></script>
}