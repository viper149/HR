@using DevExpress.AspNetCore
@using DenimERP.Reports.Inspection
@using rptSectionWiseCutPcs = DenimERP.Reports.Inspection.rptSectionWiseCutPcs

@model int

@{
    ViewData["Title"] = "Section Wise Cut-Pcs Report";
    Layout = Layout;
}
<h2>@ViewData["Title"]</h2>

@{

    @Html.DevExpress().WebDocumentViewer("WebDocumentViewer").Height("1000px").Bind(new rptSectionWiseCutPcs())
}

@section Styles {
    <partial name="_LayoutReport" />
}

@section Scripts {
    <script asp-append-version="true" src="~/js/jquery-resizable/jquery-resizable.js"></script>
}