@using DevExpress.AspNetCore
@using DenimERP.Reports.Fabric_Store
@using RPT_Fabric_Loading_Bill = DenimERP.Reports.Fabric_Store.RPT_Fabric_Loading_Bill

@model int

@{
    ViewData["Title"] = "Fabric Loading Bill Report";
    Layout = Layout;
}
<h2>@ViewData["Title"]</h2>

@{

    @Html.DevExpress().WebDocumentViewer("WebDocumentViewer").Height("1000px").Bind(new RPT_Fabric_Loading_Bill())
}

@section Styles {
    <partial name="_LayoutReport" />
}

@section Scripts {
    <script asp-append-version="true" src="~/js/jquery-resizable/jquery-resizable.js"></script>
}