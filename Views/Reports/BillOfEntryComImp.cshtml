@using DevExpress.AspNetCore
@using DenimERP.Reports.COM_IMP
@using rptBillOfEntry = DenimERP.Reports.COM_IMP.rptBillOfEntry

@model int

@{
    ViewData["Title"] = "Commercial Import";
    Layout = Layout;
}
<h2>@ViewData["Title"]</h2>

@{

    @Html.DevExpress().WebDocumentViewer("WebDocumentViewer").Height("1000px").Bind(new rptBillOfEntry())
}

@section Styles {
    <partial name="_LayoutReport" />
}

@section Scripts {
    <script asp-append-version="true" src="~/js/jquery-resizable/jquery-resizable.js"></script>
}