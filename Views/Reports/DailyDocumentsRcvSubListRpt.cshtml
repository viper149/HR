@using DevExpress.AspNetCore
@using DenimERP.Reports.COM_EX
@using rptDailyDocumentsRcvSubList = DenimERP.Reports.COM_EX.rptDailyDocumentsRcvSubList

@model int

@{
    ViewData["Title"] = "Daily Documents Received & Submission List Report";
    Layout = Layout;
}
<h2>@ViewData["Title"]</h2>

@{

    @Html.DevExpress().WebDocumentViewer("WebDocumentViewer").Height("1000px").Bind(new rptDailyDocumentsRcvSubList())
}

@section Styles {
    <partial name="_LayoutReport" />
}

@section Scripts {
    <script asp-append-version="true" src="~/js/jquery-resizable/jquery-resizable.js"></script>
}