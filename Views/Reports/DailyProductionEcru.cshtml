@using DevExpress.AspNetCore
@using DenimERP.Reports.Warping
@using rptDailyProduction_Ecru = DenimERP.Reports.Warping.rptDailyProduction_Ecru
@model string

@{
    ViewData["Title"] = "Ecru Production Delivery Sheet";
    Layout = Layout;
}
<h2>@ViewData["Title"]</h2>


@*actually this is delivery sheet*@

@{

    @Html.DevExpress().WebDocumentViewer("WebDocumentViewer").Height("1000px").Bind(new rptDailyProduction_Ecru(Model))
}

@section Styles {
    <partial name="_LayoutReport" />
}

@section Scripts {
    <script asp-append-version="true" src="~/js/jquery-resizable/jquery-resizable.js"></script>
}