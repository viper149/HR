@using DevExpress.AspNetCore
@using DenimERP.Reports.Clearance
@using rpt_Clearance_SHAD_SUMMERY = DenimERP.Reports.Clearance.rpt_Clearance_SHAD_SUMMERY

@model string

@{
    ViewData["Title"] = "Shade Wise Summery Report (Clearance)";
    Layout = Layout;
}
<h2>@ViewData["Title"]</h2>

@{

    @Html.DevExpress().WebDocumentViewer("WebDocumentViewer").Height("1000px").Bind(new rpt_Clearance_SHAD_SUMMERY())
}

@section Styles {
    <partial name="_LayoutReport" />
}

@section Scripts {
    <script asp-append-version="true" src="~/js/jquery-resizable/jquery-resizable.js"></script>
}