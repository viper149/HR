@using DevExpress.AspNetCore
@using DenimERP.Reports.Clearance
@using DenimERP.Reports.Weaving
@using rptBeamStockReport = DenimERP.Reports.Weaving.rptBeamStockReport
@model int

@{
    ViewData["Title"] = "Weaving Beam Stock Report";
    Layout = Layout;
}
<h2>@ViewData["Title"]</h2>

@{

    @Html.DevExpress().WebDocumentViewer("WebDocumentViewer").Height("1000px").Bind(new rptBeamStockReport())
}

@section Styles {
    <partial name="_LayoutReport" />
}

@section Scripts {
    <script asp-append-version="true" src="~/js/jquery-resizable/jquery-resizable.js"></script>
}