@using DevExpress.AspNetCore
@using DenimERP.Reports.Inspection
@using DepartmentWiseBulkRejectionSummery = DenimERP.Reports.Inspection.DepartmentWiseBulkRejectionSummery
@model int

@{
    ViewData["Title"] = "Department Wise Bulk Rejection Summery Report";
    Layout = Layout;
}
<h2>@ViewData["Title"]</h2>

@{

    @Html.DevExpress().WebDocumentViewer("WebDocumentViewer").Height("1000px").Bind(new DepartmentWiseBulkRejectionSummery())
}

@section Styles {
    <partial name="_LayoutReport" />
}

@section Scripts {
    <script asp-append-version="true" src="~/js/jquery-resizable/jquery-resizable.js"></script>
}