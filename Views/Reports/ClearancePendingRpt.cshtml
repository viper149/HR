@using DevExpress.AspNetCore
@using DenimERP.Reports.Clearance
@using rptClearancePending = DenimERP.Reports.Clearance.rptClearancePending

@model int

@{
    ViewData["Title"] = "Clearance Pending Report";
    Layout = Layout;
}
<h2>@ViewData["Title"]</h2>

@{

    @Html.DevExpress().WebDocumentViewer("WebDocumentViewer").Height("1000px").Bind(new rptClearancePending())
}

@section Styles {
    <partial name="_LayoutReport" />
}

@section Scripts {
    <script asp-append-version="true" src="~/js/jquery-resizable/jquery-resizable.js"></script>
}