@using DevExpress.AspNetCore
@using DenimERP.Reports.Fabric_Store
@using rptDailySampleFabricStock = DenimERP.Reports.Fabric_Store.rptDailySampleFabricStock
@model int

@{
    ViewData["Title"] = "Daily Sample Fabric Stock Report";
    Layout = Layout;
}
<h2>@ViewData["Title"]</h2>

@{

    @Html.DevExpress().WebDocumentViewer("WebDocumentViewer").Height("1000px").Bind(new rptDailySampleFabricStock())
}

@section Styles {
    <partial name="_LayoutReport" />
}

@section Scripts {
    <script asp-append-version="true" src="~/js/jquery-resizable/jquery-resizable.js"></script>
}