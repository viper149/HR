@using DevExpress.AspNetCore
@using DenimERP.Reports.Sizing
@using rptMonthlySizingProduction = DenimERP.Reports.Sizing.rptMonthlySizingProduction

@{
    ViewData["Title"] = "Sizing Monthly Production";
}
<h2>@ViewData["Title"]</h2>

@{

    @Html.DevExpress().WebDocumentViewer("WebDocumentViewer").Height("1000px").Bind(new rptMonthlySizingProduction())
}

@section Styles {
    <partial name="_LayoutReport" />
}

@section Scripts {
    <script asp-append-version="true" src="~/js/jquery-resizable/jquery-resizable.js"></script>
}