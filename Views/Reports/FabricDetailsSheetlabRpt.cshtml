@using DevExpress.AspNetCore
@using DenimERP.Views.RndProductionOrder.Reports
@using rptFabricDetailsSheet_lab = DenimERP.Views.RndProductionOrder.Reports.rptFabricDetailsSheet_lab

@model int

@{
    ViewData["Title"] = "Fabric Details Sheet For QA";
    Layout = Layout;
}
<h2>@ViewData["Title"]</h2>

@{

    @Html.DevExpress().WebDocumentViewer("WebDocumentViewer").Height("1000px").Bind(new rptFabricDetailsSheet_lab())
}

@section Styles {
    <partial name="_LayoutReport" />
}

@section Scripts {
    <script asp-append-version="true" src="~/js/jquery-resizable/jquery-resizable.js"></script>
}