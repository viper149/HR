@using DevExpress.AspNetCore
@using DenimERP.Reports.Clearance
@using rptPIWiseSOList = DenimERP.Reports.Clearance.rptPIWiseSOList


@model int

@{
    ViewData["Title"] = "PI Wise SO List Report";
    Layout = Layout;
}
<h2>@ViewData["Title"]</h2>

@{

    @Html.DevExpress().WebDocumentViewer("WebDocumentViewer").Height("1000px").Bind(new rptPIWiseSOList())
}

@section Styles {
    <partial name="_LayoutReport" />
}

@section Scripts {
    <script asp-append-version="true" src="~/js/jquery-resizable/jquery-resizable.js"></script>
}