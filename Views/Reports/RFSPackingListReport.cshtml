@using DevExpress.AspNetCore
@using DenimERP.Reports.Fabric_Store
@using rptFabricPackingList = DenimERP.Reports.Fabric_Store.rptFabricPackingList
@model int

@{
    ViewData["Title"] = "Fabric Packing List";
    Layout = Layout;
}
<h2>@ViewData["Title"]</h2>

@{

    @Html.DevExpress().WebDocumentViewer("WebDocumentViewer").Height("1000px").Bind(new rptFabricPackingList())
}

@section Styles {
    <partial name="_LayoutReport" />
}

@section Scripts {
    <script asp-append-version="true" src="~/js/jquery-resizable/jquery-resizable.js"></script>
}