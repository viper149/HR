@using DevExpress.AspNetCore
@using DenimERP.Reports.Dyeing

@model int

@{
    ViewData["Title"] = "Date Wise Dyeing Consumption Report";
    Layout = Layout;
}
<h2>@ViewData["Title"]</h2>

@{

    @Html.DevExpress().WebDocumentViewer("WebDocumentViewer").Height("1000px").Bind(new rptDateWiseDyeingConsumption())
}

@section Styles {
    <partial name="_LayoutReport" />
}

@section Scripts {
    <script asp-append-version="true" src="~/js/jquery-resizable/jquery-resizable.js"></script>
}
