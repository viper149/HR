@using DevExpress.AspNetCore
@using DenimERP.Reports.Planning
@using rptMktPersonWisePoRcv = DenimERP.Reports.Planning.rptMktPersonWisePoRcv
@model int

@{
    ViewData["Title"] = "Marketing Person Wise PO Receive Report";
    Layout = Layout;
}
<h2>@ViewData["Title"]</h2>

@{

    @Html.DevExpress().WebDocumentViewer("WebDocumentViewer").Height("1000px").Bind(new rptMktPersonWisePoRcv())
}

@section Styles {
    <partial name="_LayoutReport" />
}

@section Scripts {
    <script asp-append-version="true" src="~/js/jquery-resizable/jquery-resizable.js"></script>
}