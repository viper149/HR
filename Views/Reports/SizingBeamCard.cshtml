@using DevExpress.AspNetCore
@using DenimERP.Reports.Sizing
@using rptSizingBeamCard = DenimERP.Reports.Sizing.rptSizingBeamCard
@model string

@{
    ViewData["Title"] = "Sizing Beam Sticker";
    Layout = Layout;
}
<h2>@ViewData["Title"]</h2>

@{

    @Html.DevExpress().WebDocumentViewer("WebDocumentViewer").Height("1000px").Bind(new rptSizingBeamCard(Model))
}

@section Styles {
    <partial name="_LayoutReport" />
}

@section Scripts {
    <script asp-append-version="true" src="~/js/jquery-resizable/jquery-resizable.js"></script>
}