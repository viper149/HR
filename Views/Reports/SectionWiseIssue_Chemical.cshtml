@using DevExpress.AspNetCore
@using DenimERP.Reports.Chemical_Store
@using rptSectionWiseIssue = DenimERP.Reports.Chemical_Store.rptSectionWiseIssue
@model int

@{
    ViewData["Title"] = "Section Wise Issue Report";
    Layout = Layout;
}
<h2>@ViewData["Title"]</h2>

@{

    @Html.DevExpress().WebDocumentViewer("WebDocumentViewer").Height("1000px").Bind(new rptSectionWiseIssue())
}

@section Styles {
    <partial name="_LayoutReport" />
}

@section Scripts {
    <script asp-append-version="true" src="~/js/jquery-resizable/jquery-resizable.js"></script>
}