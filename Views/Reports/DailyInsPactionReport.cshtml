@using DevExpress.AspNetCore
@using DenimERP.Reports.Inspection
@using rptDailyInspectionReport = DenimERP.Reports.Inspection.rptDailyInspectionReport
@model int

@{
    ViewData["Title"] = "Inspection Details";
    Layout = Layout;
}
<h2>@ViewData["Title"]</h2>

@{

    @Html.DevExpress().WebDocumentViewer("WebDocumentViewer").Height("1000px").Bind(new rptDailyInspectionReport())
}

@section Styles {
    <partial name="_LayoutReport" />
}

@section Scripts {
    <script asp-append-version="true" src="~/js/jquery-resizable/jquery-resizable.js"></script>
}