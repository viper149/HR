@using DevExpress.AspNetCore
@using DenimERP.Reports.COM_IMP
@using RPT_ACCOUNTS_MUSHAK = DenimERP.Reports.COM_IMP.RPT_ACCOUNTS_MUSHAK

@model int

@{
    ViewData["Title"] = "Mushak Report";
    Layout = Layout;
}
<h2>@ViewData["Title"]</h2>

@{

    @Html.DevExpress().WebDocumentViewer("WebDocumentViewer").Height("1000px").Bind(new RPT_ACCOUNTS_MUSHAK())
}

@section Styles {
    <partial name="_LayoutReport" />
}

@section Scripts {
    <script asp-append-version="true" src="~/js/jquery-resizable/jquery-resizable.js"></script>
}