@using DevExpress.AspNetCore
@using DenimERP.Reports.Fabric_Store
@using rpt_FabricStock = DenimERP.Reports.Fabric_Store.rpt_FabricStock

@{
    ViewData["Title"] = "Fabric Stock";
}
<h2>@ViewData["Title"]</h2>

@{

    @Html.DevExpress().WebDocumentViewer("WebDocumentViewer").Height("1000px").Bind(new rpt_FabricStock())
}

@section Styles {
    <partial name="_LayoutReport" />
}

@section Scripts {
    <script asp-append-version="true" src="~/js/jquery-resizable/jquery-resizable.js"></script>
}