@using DevExpress.AspNetCore
@using DenimERP.Reports.Fabric_Store
@using RPT_Fabric_store_bin_card = DenimERP.Reports.Fabric_Store.RPT_Fabric_store_bin_card

@model int

@{
    ViewData["Title"] = "R&D Sticker";
    Layout = Layout;
}
<h2>@ViewData["Title"]</h2>

@{

    @Html.DevExpress().WebDocumentViewer("WebDocumentViewer").Height("1000px").Bind(new RPT_Fabric_store_bin_card())
}

@section Styles {
    <partial name="_LayoutReport" />
}

@section Scripts {
    <script asp-append-version="true" src="~/js/jquery-resizable/jquery-resizable.js"></script>
}