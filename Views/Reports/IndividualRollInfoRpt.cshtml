@using DevExpress.AspNetCore
@using DenimERP.Reports.FOR_ALL
@using rptIndividualRollInfo = DenimERP.Reports.FOR_ALL.rptIndividualRollInfo

@model int

@{
    ViewData["Title"] = "Individual Roll Information";
    Layout = Layout;
}
<h2>@ViewData["Title"]</h2>

@{

    @Html.DevExpress().WebDocumentViewer("WebDocumentViewer").Height("1000px").Bind(new rptIndividualRollInfo())
}

@section Styles {
    <partial name="_LayoutReport" />
}

@section Scripts {
    <script asp-append-version="true" src="~/js/jquery-resizable/jquery-resizable.js"></script>
}