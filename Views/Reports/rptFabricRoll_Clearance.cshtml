@using DevExpress.AspNetCore
@using DenimERP.Reports.Clearance
@using rptFabricRollClearance = DenimERP.Reports.Clearance.rptFabricRollClearance

@model int

@{
    ViewData["Title"] = "Fabric Roll Clearance Report";
    Layout = Layout;
}
<h2>@ViewData["Title"]</h2>

@{

    @Html.DevExpress().WebDocumentViewer("WebDocumentViewer").Height("1000px").Bind(new rptFabricRollClearance())
}

@section Styles {
    <partial name="_LayoutReport" />
}

@section Scripts {
    <script asp-append-version="true" src="~/js/jquery-resizable/jquery-resizable.js"></script>
}