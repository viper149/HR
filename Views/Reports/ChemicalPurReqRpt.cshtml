@using DevExpress.AspNetCore
@using DenimERP.Reports.Chemical_Store
@using rptChem_Pur_Requisition = DenimERP.Reports.Chemical_Store.rptChem_Pur_Requisition
@model string

@{
    ViewData["Title"] = "Chemical Purchase Requisition Report";
    Layout = Layout;
}
<h2>@ViewData["Title"]</h2>

@{

    @Html.DevExpress().WebDocumentViewer("WebDocumentViewer").Height("1000px").Bind(new rptChem_Pur_Requisition(Model))
}

@section Styles {
    <partial name="_LayoutReport" />
}

@section Scripts {
    <script asp-append-version="true" src="~/js/jquery-resizable/jquery-resizable.js"></script>
}