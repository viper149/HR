@using DevExpress.AspNetCore
@using DenimERP.Reports.Inspection
@using rpt_Ins_DailyWastageTransferReport = DenimERP.Reports.Inspection.rpt_Ins_DailyWastageTransferReport
@model int

@{
    ViewData["Title"] = "Wastage Transfer Details";
    Layout = Layout;
}
<h2>@ViewData["Title"]</h2>

@{

    @Html.DevExpress().WebDocumentViewer("WebDocumentViewer").Height("1000px").Bind(new rpt_Ins_DailyWastageTransferReport())
}

@section Styles {
    <partial name="_LayoutReport" />
}

@section Scripts {
    <script asp-append-version="true" src="~/js/jquery-resizable/jquery-resizable.js"></script>
}