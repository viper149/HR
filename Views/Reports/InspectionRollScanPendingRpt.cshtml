@using DevExpress.AspNetCore
@using DenimERP.Reports.Inspection
@using RPT_Inspection_ROLL_Scan_pending = DenimERP.Reports.Inspection.RPT_Inspection_ROLL_Scan_pending

@model int

@{
    ViewData["Title"] = "Roll Scan Pending List";
    Layout = Layout;
}
<h2>@ViewData["Title"]</h2>

@{

    @Html.DevExpress().WebDocumentViewer("WebDocumentViewer").Height("1000px").Bind(new RPT_Inspection_ROLL_Scan_pending())
}

@section Styles {
    <partial name="_LayoutReport" />
}

@section Scripts {
    <script asp-append-version="true" src="~/js/jquery-resizable/jquery-resizable.js"></script>
}