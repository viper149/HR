@using DevExpress.AspNetCore
@using DenimERP.Reports.Fabric_Store
@using RPT_DAILY_FABRIC_DELIVERY_PostCosting = DenimERP.Reports.Fabric_Store.RPT_DAILY_FABRIC_DELIVERY_PostCosting

@model int

@{
    ViewData["Title"] = "Delivery Wise Post Costing Report";
    Layout = Layout;
}
<h2>@ViewData["Title"]</h2>

@{

    @Html.DevExpress().WebDocumentViewer("WebDocumentViewer").Height("1000px").Bind(new RPT_DAILY_FABRIC_DELIVERY_PostCosting())
}

@section Styles {
    <partial name="_LayoutReport" />
}

@section Scripts {
    <script asp-append-version="true" src="~/js/jquery-resizable/jquery-resizable.js"></script>
}