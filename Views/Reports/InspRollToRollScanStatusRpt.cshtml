@using DevExpress.AspNetCore
@using DenimERP.Reports.Inspection
@using RPT_INSPECTION_Roll_to_Roll_Scan_Status = DenimERP.Reports.Inspection.RPT_INSPECTION_Roll_to_Roll_Scan_Status

@model int

@{
    ViewData["Title"] = "Roll To Roll Scan Status Report";
    Layout = Layout;
}
<h2>@ViewData["Title"]</h2>

@{

    @Html.DevExpress().WebDocumentViewer("WebDocumentViewer").Height("1000px").Bind(new RPT_INSPECTION_Roll_to_Roll_Scan_Status())
}

@section Styles {
    <partial name="_LayoutReport" />
}

@section Scripts {
    <script asp-append-version="true" src="~/js/jquery-resizable/jquery-resizable.js"></script>
}