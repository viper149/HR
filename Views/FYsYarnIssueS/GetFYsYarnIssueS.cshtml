
@model F_YS_YARN_ISSUE_MASTER_S

@{
    ViewData["Title"] = "Yarn Issue List (Sample)";
    Layout = Layout;
}

<div class="x_panel">
    <div class="x_title">
        <h2>
            <i class="fa fa-list fa-lg text-info" aria-hidden="true"></i> @ViewData["Title"]
        </h2>
        <ul class="nav navbar-right panel_toolbox">
            <li class="mr-2">
                <p>
                    <a asp-action="CreateFYsYarnIssueS" class="btn btn-primary"><i class="fa fa-plus-circle" aria-hidden="true"></i> Add</a>
                </p>
            </li>
        </ul>
        <div class="clearfix"></div>
    </div>
    <div class="x_content table-responsive">
        <table class="table table-striped" id="table">
            <thead class="thead-dark">
                <tr>
                    <th>@Html.DisplayNameFor(model => model.YISSUEDATE)</th>
                    <th>@Html.DisplayNameFor(model => model.ISSUE.ISSUTYPE)</th>
                    <th>@Html.DisplayNameFor(model => model.ISSUETO)</th>
                    <th>@Html.DisplayNameFor(model => model.YSR.YSRNO)</th>
                    <th>@Html.DisplayNameFor(model => model.PURPOSE)</th>
                    <th>@Html.DisplayNameFor(model => model.ISREMARKABLE) </th>
                    <th>@Html.DisplayNameFor(model => model.REMARKS) </th>
                    <th>
                        @Html.DisplayName("Actions")
                    </th>
                </tr>
            </thead>
        </table>
    </div>
</div>

@section Styles
{
    <link asp-append-version="true" href="~/lib/datatables/css/dataTables.bootstrap4.min.css" rel="stylesheet" />
}

@section Scripts
{
    <script asp-append-version="true" src="~/lib/datatables/js/jquery.dataTables.min.js"></script>
    <script asp-append-version="true" src="~/lib/datatables/js/dataTables.bootstrap4.min.js"></script>
    <script asp-append-version="true" src="~/js/FYsYarnIssueS/GetFYsYarnIssueS.js"></script>
}
