
@model CreateRndSampleInfoFinishViewModel

@{
    ViewData["Title"] = "Edit RnD Sample Information Finish";
    Layout = Layout;
}

<h4><i class="fa fa-edit fa-lg text-warning" aria-hidden="true"></i> Sample Info <i class="fa fa-at text-info">Finish</i></h4>
<hr />

<div class="container-fluid">
    <form id="form" asp-action="EditRndSampleInfoFinish">
        <div class="panel panel-default">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="panel-body row">

                @Html.HiddenFor(e => e.RndSampleinfoFinishing.SFINID)
                @Html.HiddenFor(e => e.RndSampleinfoFinishing.EncryptedId)

                <div class="col-lg-2 col-md-3 col-sm-6 col-xs-12">
                    <div class="form-group">
                        <label asp-for="RndSampleinfoFinishing.FINISHDATE" class="control-label"></label>
                        <input asp-for="RndSampleinfoFinishing.FINISHDATE" class="form-control" />
                        <span asp-validation-for="RndSampleinfoFinishing.FINISHDATE" class="text-danger"></span>
                    </div>
                </div>
                <div class="col-lg-2 col-md-3 col-sm-6 col-xs-12">
                    <div class="form-group">
                        <label asp-for="RndSampleinfoFinishing.PROCESSED_LENGTH" class="control-label"></label>
                        <input asp-for="RndSampleinfoFinishing.PROCESSED_LENGTH" class="form-control" />
                        <span asp-validation-for="RndSampleinfoFinishing.PROCESSED_LENGTH" class="text-danger"></span>
                    </div>
                </div>
                <div class="col-lg-2 col-md-3 col-sm-6 col-xs-12">
                    <div class="form-group">
                        <label asp-for="RndSampleinfoFinishing.LTGID" class="control-label"></label>
                        <input readonly asp-for="RndSampleinfoFinishing.LTGID" class="form-control" />
                        <span asp-validation-for="RndSampleinfoFinishing.LTGID" class="text-danger"></span>
                    </div>
                </div>
                <div class="col-lg-2 col-md-3 col-sm-6 col-xs-12">
                    <div class="form-group">
                        <label asp-for="RndSampleinfoFinishing.FINISH_ROUTE" class="control-label"></label>
                        <input asp-for="RndSampleinfoFinishing.FINISH_ROUTE" class="form-control" />
                        <span asp-validation-for="RndSampleinfoFinishing.FINISH_ROUTE" class="text-danger"></span>
                    </div>
                </div>
                <div class="col-lg-2 col-md-3 col-sm-6 col-xs-12">
                    <div class="form-group">
                        <label asp-for="RndSampleinfoFinishing.DEV_NO" class="control-label"></label>
                        <input asp-for="RndSampleinfoFinishing.DEV_NO" class="form-control" />
                        <span asp-validation-for="RndSampleinfoFinishing.DEV_NO" class="text-danger"></span>
                    </div>
                </div>
                <div class="col-lg-2 col-md-3 col-sm-6 col-xs-12">
                    <div class="form-group">
                        <label asp-for="RndSampleinfoFinishing.STYLE_NAME" class="control-label"></label>
                        <input asp-for="RndSampleinfoFinishing.STYLE_NAME" class="form-control" />
                        <span asp-validation-for="RndSampleinfoFinishing.STYLE_NAME" class="text-danger"></span>
                    </div>
                </div>
                <div class="col-lg-2 col-md-3 col-sm-6 col-xs-12">
                    <div class="form-group">
                        <label asp-for="RndSampleinfoFinishing.COLORCODE" class="control-label"></label>
                        @Html.DropDownListFor(fn => fn.RndSampleinfoFinishing.COLORCODE, new SelectList(Model.BasColors, "COLORCODE", "COLOR"), "Select A Color", htmlAttributes: new { @class = "form-control js-example-basic-single" })
                        <span asp-validation-for="RndSampleinfoFinishing.COLORCODE" class="text-danger"></span>
                    </div>
                </div>
                <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                    <div class="form-group">
                        <label asp-for="RndSampleinfoFinishing.REMARKS" class="control-label"></label>
                        <input asp-for="RndSampleinfoFinishing.REMARKS" class="form-control" />
                        <span asp-validation-for="RndSampleinfoFinishing.REMARKS" class="text-danger"></span>
                    </div>
                </div>
            </div>
            <div class="panel-body row">
                <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                    <h2 class="panel-title text-success">Buyer Requirement</h2>
                    <hr>
                </div>
                <div class="col-lg-2 col-md-3 col-sm-6 col-xs-12">
                    <div class="form-group">
                        <label asp-for="RndSampleinfoFinishing.WASHPICK" class="control-label"></label>
                        <input asp-for="RndSampleinfoFinishing.WASHPICK" class="form-control" />
                        <span asp-validation-for="RndSampleinfoFinishing.WASHPICK" class="text-danger"></span>
                    </div>
                </div>
                <div class="col-lg-2 col-md-3 col-sm-6 col-xs-12">
                    <div class="form-group">
                        <label asp-for="RndSampleinfoFinishing.GRCONST" class="control-label"></label>
                        <input asp-for="RndSampleinfoFinishing.GRCONST" class="form-control" />
                        <span asp-validation-for="RndSampleinfoFinishing.GRCONST" class="text-danger"></span>
                    </div>
                </div>
                <div class="col-lg-2 col-md-3 col-sm-6 col-xs-12">
                    <div class="form-group">
                        <label asp-for="RndSampleinfoFinishing.BWGBW" class="control-label"></label>
                        <input asp-for="RndSampleinfoFinishing.BWGBW" class="form-control" />
                        <span asp-validation-for="RndSampleinfoFinishing.BWGBW" class="text-danger"></span>
                    </div>
                </div>
                <div class="col-lg-2 col-md-3 col-sm-6 col-xs-12">
                    <div class="form-group">
                        <label asp-for="RndSampleinfoFinishing.BWGAW" class="control-label"></label>
                        <input asp-for="RndSampleinfoFinishing.BWGAW" class="form-control" />
                        <span asp-validation-for="RndSampleinfoFinishing.BWGAW" class="text-danger"></span>
                    </div>
                </div>
                <div class="col-lg-2 col-md-3 col-sm-6 col-xs-12">
                    <div class="form-group">
                        <label asp-for="RndSampleinfoFinishing.BWIBW" class="control-label"></label>
                        <input asp-for="RndSampleinfoFinishing.BWIBW" class="form-control" />
                        <span asp-validation-for="RndSampleinfoFinishing.BWIBW" class="text-danger"></span>
                    </div>
                </div>
                <div class="col-lg-2 col-md-3 col-sm-6 col-xs-12">
                    <div class="form-group">
                        <label asp-for="RndSampleinfoFinishing.BWIAW" class="control-label"></label>
                        <input asp-for="RndSampleinfoFinishing.BWIAW" class="form-control" />
                        <span asp-validation-for="RndSampleinfoFinishing.BWIAW" class="text-danger"></span>
                    </div>
                </div>
                <div class="col-lg-2 col-md-3 col-sm-6 col-xs-12">
                    <div class="form-group">
                        <label asp-for="RndSampleinfoFinishing.BSRWARP" class="control-label"></label>
                        <input asp-for="RndSampleinfoFinishing.BSRWARP" class="form-control" />
                        <span asp-validation-for="RndSampleinfoFinishing.BSRWARP" class="text-danger"></span>
                    </div>
                </div>
                <div class="col-lg-2 col-md-3 col-sm-6 col-xs-12">
                    <div class="form-group">
                        <label asp-for="RndSampleinfoFinishing.BSRWEFT" class="control-label"></label>
                        <input asp-for="RndSampleinfoFinishing.BSRWEFT" class="form-control" />
                        <span asp-validation-for="RndSampleinfoFinishing.BSRWEFT" class="text-danger"></span>
                    </div>
                </div>
                <div class="col-lg-2 col-md-3 col-sm-6 col-xs-12">
                    <div class="form-group">
                        <label asp-for="RndSampleinfoFinishing.BSTWARP" class="control-label"></label>
                        <input asp-for="RndSampleinfoFinishing.BSTWARP" class="form-control" />
                        <span asp-validation-for="RndSampleinfoFinishing.BSTWARP" class="text-danger"></span>
                    </div>
                </div>
                <div class="col-lg-2 col-md-3 col-sm-6 col-xs-12">
                    <div class="form-group">
                        <label asp-for="RndSampleinfoFinishing.BSTWEFT" class="control-label"></label>
                        <input asp-for="RndSampleinfoFinishing.BSTWEFT" class="form-control" />
                        <span asp-validation-for="RndSampleinfoFinishing.BSTWEFT" class="text-danger"></span>
                    </div>
                </div>
                <div class="col-lg-2 col-md-3 col-sm-6 col-xs-12">
                    <div class="form-group">
                        <label asp-for="RndSampleinfoFinishing.BFNEPI" class="control-label"></label>
                        <input asp-for="RndSampleinfoFinishing.BFNEPI" class="form-control" />
                        <span asp-validation-for="RndSampleinfoFinishing.BFNEPI" class="text-danger"></span>
                    </div>
                </div>
                <div class="col-lg-2 col-md-3 col-sm-6 col-xs-12">
                    <div class="form-group">
                        <label asp-for="RndSampleinfoFinishing.BFNPPI" class="control-label"></label>
                        <input asp-for="RndSampleinfoFinishing.BFNPPI" class="form-control" />
                        <span asp-validation-for="RndSampleinfoFinishing.BFNPPI" class="text-danger"></span>
                    </div>
                </div>
                <div class="col-lg-2 col-md-3 col-sm-6 col-xs-12">
                    <div class="form-group">
                        <label asp-for="RndSampleinfoFinishing.FNCONST" class="control-label"></label>
                        <textarea asp-for="RndSampleinfoFinishing.FNCONST" class="form-control" rows="1"></textarea>
                        <span asp-validation-for="RndSampleinfoFinishing.FNCONST" class="text-danger"></span>
                    </div>
                </div>

                <div class="panel-body col-lg-12 col-md-12 col-sm-12 col-xs-12" id="PreviousData">
                    @await Html.PartialAsync($"~/Views/RndSampleInfoFinish/GetPreviousDataTable.cshtml", Model)
                </div>

                <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                    <button class="btn btn-warning pull-right" type="submit">Update</button>
                    <a asp-action="GetRndSampleInfoFinish" class="btn btn-dark pull-right">Back to List</a>

                    <label class="switch pull-right mr-2 mt-1">
                        <input asp-for="RndSampleinfoFinishing.STATUS" />
                        <span class="slider round"></span>
                        <span class="absolute-no">NO</span>
                    </label>
                </div>
            </div>
        </div>
    </form>
</div>


@section Styles {
    <link asp-append-version="true" href="~/css/CustomCSS/Stepper.css" rel="stylesheet" />
}

@section Scripts {
    @{await Html.RenderPartialAsync($"_ValidationScriptPartial_Except_jQuery");}
    <script asp-append-version="true" src="~/js/custom-js/Stepper.js"></script>
    <script asp-append-version="true" src="~/lib/toastr.js/toastr.min.js"></script>
    <script asp-append-version="true" src="~/js/RndSampleInfoFinish/CreateRndSampleInfoFinish.js"></script>
}