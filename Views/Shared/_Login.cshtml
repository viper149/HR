<!DOCTYPE html>
<html>
@{
    ViewData["Provider"] = "FLEX Denim";
    ViewData["CopyRight"] = "FLEX InfoTech Limited";
}
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] | @ViewData["Provider"]</title>

    <environment include="Development">

        <link asp-append-version="true" href="~/FLEX/vendors/bootstrap/dist/css/bootstrap.css" rel="stylesheet" />
        <!-- //Meta-Tags -->
        <!-- Stylesheets -->
        <link asp-append-version="true" href="~/spin_login-form/css/font-awesome.css" rel="stylesheet">
        <!-- NProgress -->
        <link asp-append-version="true" href="~/spin_login-form/css/style.css" rel="stylesheet">
        <!--// Stylesheets -->
        <!--fonts-->
        <link asp-append-version="true" href="~/spin_login-form/css/fronts.googleapis.css" rel="stylesheet">
        <!--//fonts-->
        <!-- Custom css -->
        <link asp-append-version="true" href="~/spin_login-form/css/spin_login_form_custom.css" rel="stylesheet" />
    </environment>
    <environment include="Production">
        <link asp-append-version="true" href="~/FLEX/vendors/bootstrap/dist/css/bootstrap.css" rel="stylesheet" />
        <!-- //Meta-Tags -->
        <!-- Stylesheets -->
        <link asp-append-version="true" href="~/spin_login-form/css/font-awesome.css" rel="stylesheet">
        <!-- NProgress -->
        <link asp-append-version="true" href="~/spin_login-form/css/style.css" rel="stylesheet">
        <!--// Stylesheets -->
        <!--fonts-->
        <link asp-append-version="true" href="~/spin_login-form/css/fronts.googleapis.css" rel="stylesheet">
        <!--//fonts-->
        <!-- Custom css -->
        <link asp-append-version="true" href="~/spin_login-form/css/spin_login_form_custom.css" rel="stylesheet" />
    </environment>
    @await RenderSectionAsync("Styles", required: false)
</head>

<body class="login">
    <div id="particles-js"></div>

    <div class="col-12">
        <h1>@ViewData["Provider"]</h1>
    </div>

    @RenderBody()

    <!-- //form ends here -->
    <!--copyright-->
    <div class="copy-wthree col-12">

        <p class="text-gradient">
            ©2022-@DateTime.Now.Year | @ViewData["CopyRight"]. All Rights Reserved | @ViewData["CopyRight"].
        </p>
        <footer class="new_footer_area">
            <div class="new_footer_top">
@*
                <div class="footer_bg">
                    <div class="footer_bg_one"></div>
                    <div class="footer_bg_two"></div>
                </div>*@
            </div>
        </footer>
    </div>

    <environment include="Development">
        <script asp-append-version="true" src="~/spin_login-form/js/hide_url.js" type="text/javascript"></script>
    </environment>
    <environment exclude="Development">
        <script asp-append-version="true" src="~/spin_login-form/js/hide_url.js" type="text/javascript"></script>
    </environment>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
