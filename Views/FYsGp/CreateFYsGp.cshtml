                    <span asp-validation-for="f_YS_GP_MASTER.LC_ID" class="text-danger"></span>
                </div>
            </div>

            <div class="col-lg-2 col-md-2 col-sm-6 col-xs-12">
                <div class="form-group">
                    <label class="control-label">L/C Date </label>
                    <span class="custom-form-control-bg form-control bg-e9ecef" id="lcdate">
                    </span>

                </div>
            </div>

            <div class="col-lg-2 col-md-2 col-sm-6 col-xs-12">
                <div class="form-group">
                    <label class="control-label">Supplier Name </label>
                    <span class="custom-form-control-bg form-control bg-e9ecef" id="supp">
                    </span>

                </div>
            </div>

            <div class="col-lg-2 col-md-2 col-sm-6 col-xs-12">
