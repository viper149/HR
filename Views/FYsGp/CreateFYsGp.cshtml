                    <span asp-validation-for="f_YS_GP_MASTER.GPDATE" class="text-danger"></span>
                </div>
            </div>

            <div class="col-lg-2 col-md-2 col-sm-6 col-xs-12">
                <div class="form-group">
                    <label asp-for="cOM_IMP_LCINFORMATION.LCNO" class="control-label"></label>
                    @Html.DropDownListFor(dt => dt.f_YS_GP_MASTER.LC_ID, new SelectList(Model.LcInformationList, "LC_ID", "LCNO"),
