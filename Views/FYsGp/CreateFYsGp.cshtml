                    <input asp-for="f_YS_GP_MASTER.GPNO" class="form-control" />
                    <span asp-validation-for="f_YS_GP_MASTER.GPNO" class="text-danger"></span>
                </div>
            </div>

            <div class="col-lg-2 col-md-3 col-sm-6 col-xs-12">
                <div class="form-group">
                    <label asp-for="f_YS_GP_MASTER.GPDATE" class="control-label"></label>
