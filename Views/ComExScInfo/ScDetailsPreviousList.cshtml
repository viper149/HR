@model List<DenimERP.Models.COM_EX_SCDETAILS>

@Html.EditorFor(model => model)

@if (Model.Any())
{
    @for (int i = 0; i < Model.Count; i++)
    {
        <tr>
            <td>
                @Model[i].STYLENAME
            </td>
            <td>
                @Model[i].UNIT
            </td>
            <td>
                @Model[i].QTY
            </td>
            <td>
                @Model[i].RATE
            </td>
            <td>
                @Model[i].AMOUNT
            </td>
            <td>
                @Model[i].REMARKS
            </td>
            <td><i class="fa fa-trash fa-2x text-danger" onclick="RemoveScInfo('@Model[i].SCNO','@Model[i].STYLEID');" aria-hidden="true"></i></td>
        </tr>
    }
}