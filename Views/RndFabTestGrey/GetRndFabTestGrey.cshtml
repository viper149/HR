
@model RND_FABTEST_GREY

@{
    ViewData["Title"] = "Fabric Test Information";
    Layout = Layout;
}

<div class="x_panel">
    <div class="x_title">
        <h2><i class="fa fa-xing fa-lg text-success" aria-hidden="true"></i>@ViewData["Title"] List<i class="fa fa-at text-info">Grey</i></h2>
        <ul class="nav navbar-right panel_toolbox">
            <li class="mr-4">
                <p>
                    <a asp-action="CreateRndFabTestGrey" class="btn btn-primary"><i class="fa fa-plus-circle" aria-hidden="true"></i> Add </a>
                </p>
            </li>
            <li>
                <a class="collapse-link"><i class="fa fa-chevron-up"></i></a>
            </li>
        </ul>
        <div class="clearfix"></div>
    </div>
    <div class="x_content table-responsive table-hover" id="lcList">
        <table class="table" id="table">
            <thead>
                <tr>
                    <th>@Html.DisplayNameFor(model => model.LAB_NO)</th>
                    <th>@Html.DisplayNameFor(model => model.LTGDATE)</th>
                    <th>@Html.DisplayNameFor(model => model.OPTION1)</th>
                    <th>@Html.DisplayNameFor(model => model.SETID)</th>
                    <th>@Html.DisplayNameFor(model => model.WASHEDBY)</th>
                    <th>@Html.DisplayNameFor(model => model.UNWASHEDBY)</th>
                    <th>@Html.DisplayName("Action")</th>
                </tr>
            </thead>
        </table>
    </div>
</div>

@section Styles
{
    <link asp-append-version="true" href="~/lib/datatables/css/dataTables.bootstrap4.min.css" rel="stylesheet" />
}

@section Scripts
{
    <script asp-append-version="true" src="~/lib/datatables/js/jquery.dataTables.min.js"></script>
    <script asp-append-version="true" src="~/lib/datatables/js/dataTables.bootstrap4.min.js"></script>
    <script asp-append-version="true" src="~/js/RndFabTestGrey/GetRndFabTestGrey.js"></script>
}