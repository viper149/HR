
@model F_DYEING_PROCESS_ROPE_MASTER

@{
    ViewData["Title"] = "Dyeing Process (Rope)";
}


<div class="x_panel">
    <div class="x_title">
        <h2>
            <i class="fa fa-list fa-lg text-info" aria-hidden="true"></i> Get @ViewData["Title"]
        </h2>
        <ul class="nav navbar-right panel_toolbox">
            <li class="mr-2">
                <p>
                    <a asp-action="CreateDyeingProcessRope" class="btn btn-primary">
                        <i class="fa fa-plus-circle" aria-hidden="true"></i> Add
                    </a>
                </p>
            </li>
            <li>
                <a class="collapse-link"><i class="fa fa-chevron-up"></i></a>
            </li>
        </ul>
        <div class="clearfix"></div>
    </div>
    <div class="x_content table-responsive table-hover">
        <table class="table" id="table">
            <thead class="thead-dark">
                <tr>
                    <th scope="col">@Html.DisplayNameFor(model => model.CREATED_AT)</th>
                    <th scope="col">@Html.DisplayNameFor(model => model.TRNSDATE)</th>
                    <th scope="col">@Html.DisplayNameFor(model => model.GROUPID)</th>
                    <th scope="col">@Html.DisplayNameFor(model => model.GROUP.OPTION1)</th>
                    <th scope="col">@Html.DisplayNameFor(model => model.DYEING_CODE)</th>
                    <th scope="col">@Html.DisplayNameFor(model => model.GROUP_LENGTH)</th>
                    <th scope="col">@Html.DisplayNameFor(model => model.DYEING_LENGTH)</th>
                    <th scope="col">@Html.DisplayNameFor(model => model.REMARKS)</th>
                    <th scope="col">@Html.DisplayName("Actions")</th>
                </tr>
            </thead>
        </table>
    </div>
</div>

@section Styles
{
    <link asp-append-version="true" href="~/lib/datatables/css/dataTables.bootstrap4.min.css" rel="stylesheet" />
}

@section Scripts
{
    <script asp-append-version="true" src="~/lib/datatables/js/jquery.dataTables.min.js"></script>
    <script asp-append-version="true" src="~/lib/datatables/js/dataTables.bootstrap4.min.js"></script>
    <script asp-append-version="true" src="~/js/F_Dyeing_Process_Rope/GetDyeingProcessRope.js"></script>
}