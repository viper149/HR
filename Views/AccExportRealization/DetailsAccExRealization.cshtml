                    </strong>
                    <span class="custom-form-control form-control bg-e9ecef">
                        @Html.DisplayFor(model => model.AccExportRealization.ORQ)
                    </span>
                </div>
            </div>
            <div class="col-3">
                <div class="form-group">
                    <strong>
                        <label asp-for="AccExportRealization.CD" class="control-label"></label>
                    </strong>
                    <span class="custom-form-control form-control bg-e9ecef">
                        @Html.DisplayFor(model => model.AccExportRealization.CD)
                    </span>
                </div>
            </div>
            <div class="col-3">
                <div class="form-group">
                    <strong>
