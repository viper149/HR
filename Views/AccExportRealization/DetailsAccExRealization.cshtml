
@model AccExRealizationViewModel

@{
    ViewData["Title"] = "Account Export Realization";
}

<h2><i class="fa fa-info-circle text-info" aria-hidden="true"></i> Details @ViewData["Title"]</h2>

<div class="container-fluid">
    <div class="panel panel-default">
        <div class="panel-body row">
            <div class="pl-3 col-12">
                <h2 class="panel-title text-success">Invoice Information</h2>
            </div>
            <div class="col-3">
                <div class="form-group">
                    <strong>
                        <label asp-for="LcInfoViewModel.ComExInvoiceMaster.INVNO" class="control-label"></label>
                    </strong>
                    <span class="custom-form-control form-control bg-e9ecef">
                        @Html.DisplayFor(model => model.LcInfoViewModel.ComExInvoiceMaster.INVNO)
                    </span>
                </div>
            </div>
            <div class="col-3">
                <div class="form-group">
                    <strong>
                        <label asp-for="LcInfoViewModel.ComExLcInfo.FILENO" class="control-label"></label>
                    </strong>
                    <span class="custom-form-control form-control bg-e9ecef">
                        @Html.DisplayFor(model => model.LcInfoViewModel.ComExLcInfo.FILENO)
                    </span>
                </div>
            </div>
            <div class="col-3">
                <div class="form-group">
                    <strong>
                        <label asp-for="LcInfoViewModel.ComExInvoiceMaster.INVDATE" class="control-label"></label>
                    </strong>
                    <span class="custom-form-control form-control bg-e9ecef">
                        @Html.DisplayFor(model => model.LcInfoViewModel.ComExInvoiceMaster.INVDATE)
                    </span>
                </div>
            </div>
            <div class="col-3">
                <div class="form-group">
                    <strong>
                        <label asp-for="LcInfoViewModel.ComExInvoiceMaster.INVDURATION" class="control-label"></label>
                    </strong>
                    <span class="custom-form-control form-control bg-e9ecef">
                        @Html.DisplayFor(model => model.LcInfoViewModel.ComExInvoiceMaster.INVDURATION)
                    </span>
                </div>
            </div>
            <div class="col-3">
                <div class="form-group">
                    <strong>
                        <label asp-for="LcInfoViewModel.ComExLcInfo.LCNO" class="control-label"></label>
                    </strong>
                    <span class="custom-form-control form-control bg-e9ecef">
                        @Html.DisplayFor(model => model.LcInfoViewModel.ComExLcInfo.LCNO)
                    </span>
                </div>
            </div>
            <div class="col-3">
                <div class="form-group">
                    <strong>
                        <label asp-for="LcInfoViewModel.ComExInvoiceMaster.BUYER.BUYER_NAME" class="control-label"></label>
                    </strong>
                    <span class="custom-form-control form-control bg-e9ecef">
                        @Html.DisplayFor(model => model.LcInfoViewModel.ComExInvoiceMaster.BUYER.BUYER_NAME)
                    </span>
                </div>
            </div>
            <div class="col-3">
                <div class="form-group">
                    <strong>
                        <label asp-for="LcInfoViewModel.ComExInvoiceMaster.PDOCNO" class="control-label"></label>
                    </strong>
                    <span class="custom-form-control form-control bg-e9ecef">
                        @Html.DisplayFor(model => model.LcInfoViewModel.ComExInvoiceMaster.PDOCNO)
                    </span>
                </div>
            </div>
            <div class="col-3">
                <div class="form-group">
                    <strong>
                        <label asp-for="LcInfoViewModel.ComExInvoiceMaster.DOC_NOTES" class="control-label"></label>
                    </strong>
                    <span class="custom-form-control form-control bg-e9ecef">
                        @Html.DisplayFor(model => model.LcInfoViewModel.ComExInvoiceMaster.DOC_NOTES)
                    </span>
                </div>
            </div>
            <div class="col-3">
                <div class="form-group">
                    <strong>
                        <label asp-for="LcInfoViewModel.ComExLcInfo.BANK_.PARTY_BANK" class="control-label"></label>
                    </strong>
                    <span class="custom-form-control form-control bg-e9ecef">
                        @Html.DisplayFor(model => model.LcInfoViewModel.ComExLcInfo.BANK_.PARTY_BANK)
                    </span>
                </div>
            </div>
            <div class="col-3">
                <div class="form-group">
                    <strong>
                        <label asp-for="LcInfoViewModel.ComExLcInfo.BANK.BEN_BANK" class="control-label"></label>
                    </strong>
                    <span class="custom-form-control form-control bg-e9ecef">
                        @Html.DisplayFor(model => model.LcInfoViewModel.ComExLcInfo.BANK.BEN_BANK)
                    </span>
                </div>
            </div>
            <div class="col-3">
                <div class="form-group">
                    <strong>
                        <label asp-for="LcInfoViewModel.ComExInvoiceMaster.NEGODATE" class="control-label"></label>
                    </strong>
                    <span class="custom-form-control form-control bg-e9ecef">
                        @Html.DisplayFor(model => model.LcInfoViewModel.ComExInvoiceMaster.NEGODATE)
                    </span>
                </div>
            </div>
            <div class="col-3">
                <div class="form-group">
                    <strong>
                        <label asp-for="LcInfoViewModel.ComExInvoiceMaster.STATUS" class="control-label"></label>
                    </strong>
                    <span class="custom-form-control form-control bg-e9ecef">
                        @if (Model.LcInfoViewModel.ComExInvoiceMaster.ISACTIVE)
                        {
                            @Html.DisplayName("Active")
                        }
                        else
                        {
                            @Html.DisplayName("Inactive")
                        }
                    </span>
                </div>
            </div>
            @{
                if (Model.LcInfoViewModel.ComExInvDetails.Any())
                {
                    <div class="col-12">
                        <hr />
                        <h2 class="panel-title text-success">Style Information</h2>

                        <table class="table">
                            <thead>
                                <tr>
                                    <th>@Html.DisplayNameFor(model => model.LcInfoViewModel.ComExInvDetails.FirstOrDefault().ComExFabstyle.FABCODENavigation.STYLE_NAME)</th>
                                    <th>@Html.DisplayNameFor(model => model.LcInfoViewModel.ComExInvDetails.FirstOrDefault().ROLL)</th>
                                    <th>@Html.DisplayNameFor(model => model.LcInfoViewModel.ComExInvDetails.FirstOrDefault().QTY)</th>
                                    <th>@Html.DisplayNameFor(model => model.LcInfoViewModel.ComExInvDetails.FirstOrDefault().RATE)</th>
                                    <th>@Html.DisplayNameFor(model => model.LcInfoViewModel.ComExInvDetails.FirstOrDefault().AMOUNT)</th>
                                    <th>@Html.DisplayNameFor(model => model.LcInfoViewModel.ComExInvDetails.FirstOrDefault().REMARKS)</th>
                                </tr>
                            </thead>
                            <tbody>
                                @for (int i = 0; i < Model.LcInfoViewModel.ComExInvDetails.Count; i++)
                                {
                                    <tr>
                                        <td>@Model.LcInfoViewModel.ComExInvDetails[i].ComExFabstyle.FABCODENavigation.STYLE_NAME</td>
                                        <td>@Model.LcInfoViewModel.ComExInvDetails[i].ROLL</td>
                                        <td>@Model.LcInfoViewModel.ComExInvDetails[i].QTY</td>
                                        <td>@Model.LcInfoViewModel.ComExInvDetails[i].RATE</td>
                                        <td>@Model.LcInfoViewModel.ComExInvDetails[i].AMOUNT</td>
                                        <td>@Model.LcInfoViewModel.ComExInvDetails[i].REMARKS</td>
                                    </tr>
                                }
                            </tbody>
                        </table>
                    </div>
                }
            }

            <div class="col-12">
                <h2 class="panel-title text-success">Realization</h2>
            </div>

            <div class="col-3">
                <div class="form-group">
                    <strong>
                        <label asp-for="AccExportRealization.REZDATE" class="control-label"></label>
                    </strong>
                    <span class="custom-form-control form-control bg-e9ecef">
                        @Html.DisplayFor(model => model.AccExportRealization.REZDATE)
                    </span>
                </div>
            </div>
            <div class="col-3">
                <div class="form-group">
                    <strong>
                        <label asp-for="AccExportRealization.PRC_USD" class="control-label"></label>
                    </strong>
                    <span class="custom-form-control form-control bg-e9ecef">
                        @Html.DisplayFor(model => model.AccExportRealization.REZDATE)
                    </span>
                </div>
            </div>
            <div class="col-3">
                <div class="form-group">
                    <strong>
                        <label asp-for="AccExportRealization.PRC_EURO" class="control-label"></label>
                    </strong>
                    <span class="custom-form-control form-control bg-e9ecef">
                        @Html.DisplayFor(model => model.AccExportRealization.PRC_EURO)
                    </span>
                </div>
            </div>
            <div class="col-3">
                <div class="form-group">
                    <strong>
                        <label asp-for="AccExportRealization.ERQ" class="control-label"></label>
                    </strong>
                    <span class="custom-form-control form-control bg-e9ecef">
                        @Html.DisplayFor(model => model.AccExportRealization.ERQ)
