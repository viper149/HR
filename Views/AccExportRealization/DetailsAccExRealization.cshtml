                    </strong>
                    <span class="custom-form-control form-control bg-e9ecef">
                        @Html.DisplayFor(model => model.AccExportRealization.REMARKS)
                    </span>
                </div>
            </div>
        </div>
    </div>
</div>

<div class="col-md-12">
    @Html.ActionLink("Edit", $"EditAccExRealization", new { trnsId = Model.AccExportRealization.EncryptedId }, new { @class = "btn btn-warning pull-right" })
    <a asp-action="GetAccExRealizationWithPaged" class="btn btn-dark pull-right" accesskey="b">Back to List</a>
</div>