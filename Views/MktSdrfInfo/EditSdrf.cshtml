                                </label>
                            </div>
                            <span asp-validation-for="MktSdrfInfo.PLANT_HEAD_APPROVE" class="text-danger"></span>
                        </div>
                    </div>
                }

                <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                    <button class="btn btn-warning pull-right" type="submit">Update</button>
                    <a asp-action="GetSdrfList" class="btn btn-dark pull-right">Back to List</a>
                </div>
            </div>
