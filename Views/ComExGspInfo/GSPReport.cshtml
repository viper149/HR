@using DevExpress.AspNetCore
@using DenimERP.Views.ComExGspInfo.Reports
@using rptBTMA_GSP = DenimERP.Views.ComExGspInfo.Reports.rptBTMA_GSP
@model string

@{
    ViewData["Title"] = "GSP Report";
    Layout = Layout;
}
<h2>@ViewData["Title"]</h2>

@{

    @Html.DevExpress().WebDocumentViewer("WebDocumentViewer").Height("1000px").Bind(new rptBTMA_GSP(Model))
}

@section Styles {
    <partial name="_LayoutReport" />
}

@section Scripts {
    <script asp-append-version="true" src="~/js/jquery-resizable/jquery-resizable.js"></script>
}