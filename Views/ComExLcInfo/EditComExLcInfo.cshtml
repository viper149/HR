                            <label asp-for="PREV_YEAR" class="control-label fa fa-history btn-link"></label>
                            <div class="form-control custom-form-control">
                                <label class="switch">
                                    <input asp-for="PREV_YEAR" />
                                    <span class="slider round"></span>
                                    <span class="absolute-no">NO</span>
                                </label>
                            </div>
                            <span asp-validation-for="PREV_YEAR" class="text-danger"></span>
