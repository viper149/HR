@model CreateComExLcInfoViewModel

@{
    ViewData["Title"] = "Com. Export LC Information";
}

<div class="container-fluid">

    <h2><i class="fa fa-edit text-warning" aria-hidden="true"></i> Update @ViewData["Title"]</h2>

    <div class="card border border-info">
        <div class="card-header text-right">
            <h2 class="card-title">@Html.DisplayNameFor(e => e.ComExLcinfo.FILENO)</h2>
            <p class="card-subtitle btn-link">@Model.ComExLcinfo.FILENO</p>
        </div>
    </div>

    <div class="stepwizard">
        <div class="stepwizard-row setup-panel d-flex justify-content-around">
            <div class="stepwizard-step">
                <a href="#step-1" type="button" class="btn btn-success btn-circle">1</a>
                <p><small>LC Information - 1</small></p>
            </div>
            <div class="stepwizard-step">
                <a href="#step-2" type="button" class="btn btn-default btn-circle border-1" disabled="disabled">2</a>
                <p><small>LC Information - 2</small></p>
            </div>
            <div class="stepwizard-step">
                <a href="#step-3" type="button" class="btn btn-default btn-circle" disabled="disabled">3</a>
                <p><small>Finish</small></p>
            </div>
        </div>
    </div>

