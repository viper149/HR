@model LoomSettingsSampleViewModel

@{
    ViewData["Title"] = "Loom Settings Information (Sample)";
}

<h2><i class="fa fa-edit text-success" aria-hidden="true"></i> Update @ViewData["Title"]</h2>

<div class="container-fluid">
    <form id="form" enctype="multipart/form-data" role="form" asp-action="EditLoomSettingsSample">
        <div asp-validation-summary="ModelOnly" class="text-danger"></div>
        <div class="panel-body row">
            @Html.HiddenFor(e => e.LoomSettingsSample.EncryptedId)
            <div class="col-lg-2 col-md-4 col-sm-6 col-xs-12">
                <div class="form-group">
                    <label asp-for="LoomSettingsSample.DEV_ID" class="control-label"></label>
                    @Html.DropDownListFor(dt => dt.LoomSettingsSample.DEV_ID, new SelectList(Model.RndSampleInfoWeavingsList, "WVID", "FABCODE"), "Select Development No.", new { @class = "form-control js-example-basic-single" })
                    <span asp-validation-for="LoomSettingsSample.DEV_ID" class="text-danger"></span>
                </div>
            </div>
            <div class="col-lg-12 col-md-6 col-sm-6 col-xs-12 pull-left">
                <h6 class="panel-title text-info"> Details Info </h6>
            </div>
            <div class="col-lg-2 col-md-3 col-sm-6 col-xs-12">
                <div class="form-group">
                    <label asp-for="LoomSettingsSample.OPT1" class="control-label"></label>
                    <select asp-for="LoomSettingsSample.OPT1" class="form-control js-example-basic-single">
                        <option value="" selected="selected">Select Shift</option>
                        <option value="2">A</option>
                        <option value="3">B</option>
                        <option value="4">C</option>
                    </select>
                    <span asp-validation-for="LoomSettingsSample.OPT1" class="text-danger"></span>
                </div>
            </div>
            <div class="col-lg-2 col-md-3 col-sm-4 col-xs-12">
                <div class="form-group">
                    <label class="control-label">Set/Prog. No</label>
                    <span class="custom-form-control-bg form-control bg-e9ecef" id="setNo">
                    </span>
                </div>
            </div>
            <div class="col-lg-2 col-md-3 col-sm-4 col-xs-12">
                <div class="form-group">
                    <label class="control-label">Color</label>
                    <span class="custom-form-control-bg form-control bg-e9ecef" id="color">
                    </span>
                </div>
            </div>
            <div class="col-lg-8 col-md-12 col-sm-12 col-xs-12">
                <div class="form-group">
                    <label asp-for="LoomSettingsSample.GRCONST" class="control-label"></label>
                    <input asp-for="LoomSettingsSample.GRCONST" class="form-control" readonly="readonly" />
                    <span asp-validation-for="LoomSettingsSample.GRCONST" class="text-danger"></span>
                </div>
            </div>
            <div class="col-lg-4 col-md-6 col-sm-12 col-xs-12">
                <div class="form-group">
                    <label asp-for="LoomSettingsSample.WEFT_COUNT" class="control-label"></label>
                    <input asp-for="LoomSettingsSample.WEFT_COUNT" class="form-control" readonly="readonly" />
                    <span asp-validation-for="LoomSettingsSample.WEFT_COUNT" class="text-danger"></span>
                </div>
            </div>
            <div class="col-lg-2 col-md-3 col-sm-4 col-xs-12">
                <div class="form-group">
                    <label asp-for="LoomSettingsSample.WEFT_RATIO" class="control-label"></label>
                    <input asp-for="LoomSettingsSample.WEFT_RATIO" class="form-control" readonly="readonly" />
                    <span asp-validation-for="LoomSettingsSample.WEFT_RATIO" class="text-danger"></span>
                </div>
            </div>
            <div class="col-lg-2 col-md-3 col-sm-4 col-xs-12">
                <div class="form-group">
                    <label asp-for="LoomSettingsSample.WEFT_LOT" class="control-label"></label>
                    <input asp-for="LoomSettingsSample.WEFT_LOT" class="form-control" readonly="readonly" />
                    <span asp-validation-for="LoomSettingsSample.WEFT_LOT" class="text-danger"></span>
                </div>
            </div>
            <div class="col-lg-2 col-md-3 col-sm-4 col-xs-12">
                <div class="form-group">
                    <label asp-for="LoomSettingsSample.WEFT_SUPP" class="control-label"></label>
                    <input asp-for="LoomSettingsSample.WEFT_SUPP" class="form-control" readonly="readonly" />
                    <span asp-validation-for="LoomSettingsSample.WEFT_SUPP" class="text-danger"></span>
                </div>
            </div>
            <div class="col-lg-2 col-md-3 col-sm-4 col-xs-12">
                <div class="form-group">
                    <label class="control-label">Loom Type</label>
                    <span class="custom-form-control-bg form-control bg-e9ecef" id="loom">
                    </span>
                </div>
            </div>
            <div class="col-lg-2 col-md-3 col-sm-4 col-xs-12">
                <div class="form-group">
                    <label class="control-label">Weave</label>
                    <span class="custom-form-control-bg form-control bg-e9ecef" id="weave">
                    </span>
                </div>
            </div>
            <div class="col-lg-2 col-md-3 col-sm-4 col-xs-12">
                <div class="form-group">
                    <label class="control-label">Reed Count/Dent</label>
                    <span class="custom-form-control-bg form-control bg-e9ecef" id="reedCount">
                    </span>
                </div>
            </div>
            <div class="col-lg-2 col-md-3 col-sm-4 col-xs-12">
                <div class="form-group">
                    <label class="control-label">Total Ends</label>
                    <span class="custom-form-control-bg form-control bg-e9ecef" id="totalEnds">
                    </span>
                </div>
            </div>
            <div class="col-lg-2 col-md-3 col-sm-4 col-xs-12">
                <div class="form-group">
                    <label class="control-label">Reed Space</label>
                    <span class="custom-form-control-bg form-control bg-e9ecef" id="rs">
                    </span>
                </div>
            </div>
            <div class="col-lg-2 col-md-3 col-sm-6 col-xs-12">
                <div class="form-group">
                    <label asp-for="LoomSettingsSample.WARP_CRIMP" class="control-label"></label>
                    <input asp-for="LoomSettingsSample.WARP_CRIMP" class="form-control" />
                    <span asp-validation-for="LoomSettingsSample.WARP_CRIMP" class="text-danger"></span>
                </div>
            </div>
            <div class="col-lg-2 col-md-3 col-sm-6 col-xs-12">
                <div class="form-group">
                    <label asp-for="LoomSettingsSample.WEFT_CRIMP" class="control-label"></label>
                    <input asp-for="LoomSettingsSample.WEFT_CRIMP" class="form-control" />
                    <span asp-validation-for="LoomSettingsSample.WEFT_CRIMP" class="text-danger"></span>
                </div>
            </div>
            <div class="col-lg-2 col-md-3 col-sm-6 col-xs-12">
                <div class="form-group">
                    <label asp-for="LoomSettingsSample.FABRIC_LENGTH" class="control-label"></label>
                    <input asp-for="LoomSettingsSample.FABRIC_LENGTH" class="form-control" />
                    <span asp-validation-for="LoomSettingsSample.FABRIC_LENGTH" class="text-danger"></span>
                </div>
            </div>
            <div class="col-lg-2 col-md-3 col-sm-6 col-xs-12">
                <div class="form-group">
                    <label asp-for="LoomSettingsSample.RPM" class="control-label"></label>
                    <input asp-for="LoomSettingsSample.RPM" class="form-control" />
                    <span asp-validation-for="LoomSettingsSample.RPM" class="text-danger"></span>
                </div>
            </div>
            <div class="col-lg-2 col-md-3 col-sm-6 col-xs-12">
                <div class="form-group">
                    <label asp-for="LoomSettingsSample.SEL_WEAVE" class="control-label"></label>
                    <input asp-for="LoomSettingsSample.SEL_WEAVE" class="form-control" />
                    <span asp-validation-for="LoomSettingsSample.SEL_WEAVE" class="text-danger"></span>
                </div>
            </div>
            <div class="col-lg-2 col-md-3 col-sm-6 col-xs-12">
                <div class="form-group">
                    <label asp-for="LoomSettingsSample.SEL_WIDTH" class="control-label"></label>
                    <input asp-for="LoomSettingsSample.SEL_WIDTH" class="form-control" />
                    <span asp-validation-for="LoomSettingsSample.SEL_WIDTH" class="text-danger"></span>
                </div>
            </div>
            <div class="col-lg-2 col-md-3 col-sm-6 col-xs-12">
                <div class="form-group">
                    <label asp-for="LoomSettingsSample.EFFICIENCE" class="control-label"></label>
                    <input asp-for="LoomSettingsSample.EFFICIENCE" class="form-control" />
                    <span asp-validation-for="LoomSettingsSample.EFFICIENCE" class="text-danger"></span>
                </div>
            </div>

            <div class="col-lg-12 col-md-6 col-sm-6 col-xs-12 pull-left">
                <h6 class="panel-title text-info"> Breaks/CMPX </h6>
            </div>
            <div class="col-lg-2 col-md-3 col-sm-6 col-xs-12">
                <div class="form-group">
                    <label asp-for="LoomSettingsSample.BR_WARP" class="control-label"></label>
                    <input asp-for="LoomSettingsSample.BR_WARP" class="form-control" />
                    <span asp-validation-for="LoomSettingsSample.BR_WARP" class="text-danger"></span>
                </div>
            </div>
            <div class="col-lg-2 col-md-3 col-sm-6 col-xs-12">
                <div class="form-group">
                    <label asp-for="LoomSettingsSample.BR_WEFT" class="control-label"></label>
                    <input asp-for="LoomSettingsSample.BR_WEFT" class="form-control" />
                    <span asp-validation-for="LoomSettingsSample.BR_WEFT" class="text-danger"></span>
                </div>
            </div>
            <div class="col-lg-2 col-md-3 col-sm-6 col-xs-12">
                <div class="form-group">
                    <label asp-for="LoomSettingsSample.BR_CATCH_CORD" class="control-label"></label>
                    <input asp-for="LoomSettingsSample.BR_CATCH_CORD" class="form-control" />
                    <span asp-validation-for="LoomSettingsSample.BR_CATCH_CORD" class="text-danger"></span>
                </div>
            </div>
            <div class="col-lg-2 col-md-3 col-sm-6 col-xs-12">
                <div class="form-group">
                    <label asp-for="LoomSettingsSample.BR_OTHER_BREAK" class="control-label"></label>
                    <input asp-for="LoomSettingsSample.BR_OTHER_BREAK" class="form-control" />
                    <span asp-validation-for="LoomSettingsSample.BR_OTHER_BREAK" class="text-danger"></span>
                </div>
            </div>
            <div class="col-lg-2 col-md-3 col-sm-6 col-xs-12">
                <div class="form-group">
                    <label class="control-label" readonly="readonly">Total</label>
                    <input class="form-control" />
                </div>
            </div>
            <div class="col-lg-12 col-md-6 col-sm-6 col-xs-12 pull-left">
                <h6 class="panel-title text-info"> Others </h6>
            </div>
            <div class="col-lg-2 col-md-3 col-sm-6 col-xs-12">
                <div class="form-group">
                    <label asp-for="LoomSettingsSample.BACK_HEIGHT" class="control-label"></label>
                    <input asp-for="LoomSettingsSample.BACK_HEIGHT" class="form-control" />
                    <span asp-validation-for="LoomSettingsSample.BACK_HEIGHT" class="text-danger"></span>
                </div>
            </div>
            <div class="col-lg-2 col-md-3 col-sm-6 col-xs-12">
                <div class="form-group">
                    <label asp-for="LoomSettingsSample.BACK_DEPTH" class="control-label"></label>
                    <input asp-for="LoomSettingsSample.BACK_DEPTH" class="form-control" />
                    <span asp-validation-for="LoomSettingsSample.BACK_DEPTH" class="text-danger"></span>
                </div>
            </div>
            <div class="col-lg-2 col-md-3 col-sm-6 col-xs-12">
                <div class="form-group">
                    <label asp-for="LoomSettingsSample.D_HEIGHT" class="control-label"></label>
                    <input asp-for="LoomSettingsSample.D_HEIGHT" class="form-control" />
                    <span asp-validation-for="LoomSettingsSample.D_HEIGHT" class="text-danger"></span>
                </div>
            </div>
            <div class="col-lg-2 col-md-3 col-sm-6 col-xs-12">
                <div class="form-group">
                    <label asp-for="LoomSettingsSample.D_DEPTH" class="control-label"></label>
                    <input asp-for="LoomSettingsSample.D_DEPTH" class="form-control" />
                    <span asp-validation-for="LoomSettingsSample.D_DEPTH" class="text-danger"></span>
                </div>
            </div>
            <div class="col-lg-2 col-md-3 col-sm-6 col-xs-12">
                <div class="form-group">
                    <label asp-for="LoomSettingsSample.WARP_TENSN" class="control-label"></label>
                    <input asp-for="LoomSettingsSample.WARP_TENSN" class="form-control" />
                    <span asp-validation-for="LoomSettingsSample.WARP_TENSN" class="text-danger"></span>
                </div>
            </div>
            <div class="col-lg-2 col-md-3 col-sm-6 col-xs-12">
                <div class="form-group">
                    <label asp-for="LoomSettingsSample.FR_HEIGHT" class="control-label"></label>
                    <input asp-for="LoomSettingsSample.FR_HEIGHT" class="form-control" />
                    <span asp-validation-for="LoomSettingsSample.FR_HEIGHT" class="text-danger"></span>
                </div>
            </div>
            <div class="col-lg-2 col-md-3 col-sm-6 col-xs-12">
                <div class="form-group">
                    <label asp-for="LoomSettingsSample.SHADE_ANGLE" class="control-label"></label>
                    <input asp-for="LoomSettingsSample.SHADE_ANGLE" class="form-control" />
                    <span asp-validation-for="LoomSettingsSample.SHADE_ANGLE" class="text-danger"></span>
                </div>
            </div>
            <div class="col-lg-2 col-md-3 col-sm-6 col-xs-12">
                <div class="form-group">
                    <label asp-for="LoomSettingsSample.SHADE_CROSSING" class="control-label"></label>
                    <input asp-for="LoomSettingsSample.SHADE_CROSSING" class="form-control" />
                    <span asp-validation-for="LoomSettingsSample.SHADE_CROSSING" class="text-danger"></span>
                </div>
            </div>
            <div class="col-lg-2 col-md-3 col-sm-6 col-xs-12">
                <div class="form-group">
                    <label asp-for="LoomSettingsSample.REMARKS" class="control-label"></label>
                    <input asp-for="LoomSettingsSample.REMARKS" class="form-control" />
                    <span asp-validation-for="LoomSettingsSample.REMARKS" class="text-danger"></span>
                </div>
            </div>

            <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                <button id="btn_submit" class="btn btn-warning nextBtn pull-right" type="submit">Update</button>
                <a asp-action="GetLoomSettingsSample" class="btn btn-dark pull-right">Back to List</a>
            </div>
        </div>
    </form>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync($"_ValidationScriptPartial_Except_jQuery");}
    <script asp-append-version="true" src="~/js/LoomSettingsSample/CreateLoomSettingsSample.js"></script>
}
