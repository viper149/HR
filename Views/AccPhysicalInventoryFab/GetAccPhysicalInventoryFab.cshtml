@model ACC_PHYSICAL_INVENTORY_FAB

@{
    <div class="x_panel">
        <div class="x_title">
            <h2><i class="fa fa-xing fa-lg text-success" aria-hidden="true"></i>Roll List</h2>
            <ul class="nav navbar-right panel_toolbox">
                <li>
                    <a class="collapse-link"><i class="fa fa-chevron-up"></i></a>
                </li>
            </ul>
            <div class="clearfix"></div>
        </div>
        <div class="x_content table-responsive table-hover">
            <table class="table" id="table">
                <thead>
                    <tr>
                        <th>@Html.DisplayNameFor(model => model.FPI_ID)</th>
                        <th>@Html.DisplayNameFor(model => model.INDEX)</th>
                        <th>@Html.DisplayNameFor(model => model.FPI_DATE)</th>
                        <th>@Html.DisplayNameFor(model => model.ROLL_NO)</th>
                        <th>@Html.DisplayNameFor(model => model.ROLL_.FABCODE)</th>
                        <th>@Html.DisplayNameFor(model => model.ROLL_.ROLL_.FAB_GRADE)</th>
                    </tr>
                </thead>
            </table>
        </div>
    </div>
}