

@using DevExpress.AspNetCore
@using DenimERP.Views.ComExPiMaster.Reports
@using RptAuditSummary = DenimERP.Views.ComExPiMaster.Reports.RptAuditSummary
@model Dictionary<int, string>

@{
    ViewData["Title"] = "Report for Audit Summary";
    Layout = Layout;
}

<h2>Production Order</h2>

@{

    @Html.DevExpress().WebDocumentViewer("WebDocumentViewer").Height("1000px").Bind(new RptAuditSummary(Model))
}

@section Styles {
    <partial name="_LayoutReport" />
}

@section Scripts {
    <script asp-append-version="true" src="~/js/jquery-resizable/jquery-resizable.js"></script>
}