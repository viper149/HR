                    <span class="custom-form-control form-control bg-e9ecef">
                        @Html.DisplayFor(model => model.UPDATED_AT)
                    </span>
                </div>
            </div>
        </div>
    </div>
</div>

<div>
    <a asp-action="GetCosFixedCostWithPaged" class="btn btn-dark">Back to List</a>
</div>