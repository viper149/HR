    Layout = Layout;
}

<h2><i class="fa fa-info-circle fa-lg text-info" aria-hidden="true"></i> Fixed Cost</h2>
<hr />

<div class="container-fluid">
    <div class="panel panel-default">
        <div class="panel-body row">
            <div class="col-3">
                <div class="form-group">
                    <strong>
                        <label asp-for="DYEINGCOST" class="control-label"></label>
                    </strong>
                    <span class="custom-form-control form-control bg-e9ecef">
                        @Html.DisplayFor(model => model.DYEINGCOST)
                    </span>
                </div>
            </div>
            <div class="col-3">
                <div class="form-group">
                    <strong>
                        <label asp-for="UPCHARGE" class="control-label"></label>
                    </strong>
                    <span class="custom-form-control form-control bg-e9ecef">
                        @Html.DisplayFor(model => model.UPCHARGE)
                    </span>
                </div>
            </div>
            <div class="col-3">
                <div class="form-group">
                    <strong>
                        <label asp-for="PRINTINGCOST" class="control-label"></label>
                    </strong>
                    <span class="custom-form-control form-control bg-e9ecef">
                        @Html.DisplayFor(model => model.PRINTINGCOST)
                    </span>
                </div>
            </div>
            <div class="col-3">
                <div class="form-group">
                    <strong>
                        <label asp-for="OVERHEADCOST" class="control-label"></label>
                    </strong>
                    <span class="custom-form-control form-control bg-e9ecef">
                        @Html.DisplayFor(model => model.OVERHEADCOST)
                    </span>
                </div>
            </div>
            <div class="col-3">
