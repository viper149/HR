                <div class="form-group">
                    <strong>
                        <label asp-for="STATUS" class="control-label"></label>
                    </strong>
                    <span class="custom-form-control form-control bg-e9ecef">
                        @if (Model.STATUS != null)
                        {
                            <span class="@Model.STATUS?text-success:text-danger"> @Html.DisplayName((bool)Model.STATUS ? "Active" : "Inactive")</span>
                        }
                        else
                        {
                            <span class="text-danger"> @Html.DisplayName("Inactive")</span>
                        }
                    </span>
                </div>
            </div>
            <div class="col-3">
                <div class="form-group">
