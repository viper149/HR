
@model ComImpLcInformationEditViewModel

@if (Model.PrevComImpLcdetailses.Any())
{
    @for (int i = 0; i < Model.PrevComImpLcdetailses.Count; i++)
    {
        <tr>
            <td>@Html.DisplayFor(e => e.PrevComImpLcdetailses[i].TRNSDATE)</td>
            <td>@Model.PrevComImpLcdetailses[i].PINO</td>
            <td>@Html.DisplayFor(e => e.PrevComImpLcdetailses[i].PIDATE)</td>
            <td><a class="text-primary" href="~/files/lc_files/?fileName=@Model.PrevComImpLcdetailses[i].PIPATH" style="text-decoration: underline;" target="_blank">@Model.PrevComImpLcdetailses[i].PIPATH.Split(new char[] { ' ', '_' }, 2)[1]</a></td>
            <td>@Model.bAS_PRODCATEGORies.Where(e => e.CATID.Equals(Model.PrevComImpLcdetailses[i].PRODID)).Select(e => e.CATEGORY).FirstOrDefault()</td>
            <td>@Model.PrevComImpLcdetailses[i].UNIT</td>
            <td>@Model.PrevComImpLcdetailses[i].QTY</td>
            <td>@Model.PrevComImpLcdetailses[i].RATE</td>
            <td>@Model.PrevComImpLcdetailses[i].TOTAL</td>
            <td><i class="fa fa-trash fa-lg text-danger" onclick="removeImportLcDetailsPreviousList('@Model.PrevComImpLcdetailses[i].EncryptedId', '@Model.cOM_IMP_LCINFORMATION.EncryptedId')" aria-hidden="true"></i></td>
        </tr>
    }
}