
@model COM_IMP_LCINFORMATION

@{
    ViewData["Title"] = "Commercial LC";
}

<div class="x_panel">
    <div class="x_title">
        <h2>
            <i class="fa fa-list fa-lg text-info"></i> @ViewData["Title"] <i class="fa fa-at text-info">Import</i>
        </h2>
        <ul class="nav navbar-right panel_toolbox">
            <li class="mr-2">
                <p><a asp-action="CreateComImpLcInformation" class="btn btn-primary"><i class="fa fa-plus-circle" aria-hidden="true"></i> Add </a></p>
            </li>
            <li class="mr-2">
                <p><a asp-action="RGetComImpLcInformationList" class="btn btn-info"><i class="fa fa-print" aria-hidden="true"></i> Reports</a></p>
            </li>
            <li>
                <a class="collapse-link"><i class="fa fa-chevron-up"></i></a>
            </li>
        </ul>
        <div class="clearfix"></div>
    </div>
    <div class="x_content table-responsive table-hover" id="lcList">
        <table class="table" id="table">
            <thead class="thead-dark">
                <tr>
                    <th>@Html.DisplayNameFor(model => model.LC_ID)</th>
                    <th>@Html.DisplayNameFor(model => model.LCNO)</th>
                    <th>@Html.DisplayNameFor(model => model.LCDATE)</th>
                    <th>Master L/C</th>
                    <th>Master L/C Date</th>
                    <th>Master L/C Value</th>
                    <th>Lien Value</th>
                    <th>@Html.DisplayNameFor(model => model.COM_IMP_LCTYPE.TYPENAME)</th>
                    <th>@Html.DisplayNameFor(model => model.CURRENCY)</th>
                    <th>@Html.DisplayNameFor(model => model.SHIPDATE)</th>
                    <th>@Html.DisplayNameFor(model => model.EXPDATE)</th>
                    <th>@Html.DisplayNameFor(model => model.SUPP.SUPPNAME)</th>
                    <th>@Html.DisplayNameFor(model => model.CAT.CATEGORY)</th>
                    <th>@Html.DisplayName("File")</th>
                    <th>@Html.DisplayName("Actions")</th>
                </tr>
            </thead>
        </table>
    </div>
</div>


@section Styles
{
    <link asp-append-version="true" href="~/lib/datatables/css/dataTables.bootstrap4.min.css" rel="stylesheet" />
}

@section Scripts
{
    <script asp-append-version="true" src="~/lib/datatables/js/jquery.dataTables.min.js"></script>
    <script asp-append-version="true" src="~/lib/datatables/js/dataTables.bootstrap4.min.js"></script>
    <script asp-append-version="true" src="~/js/comImpLC/GetComImpLcInformation.js"></script>
}