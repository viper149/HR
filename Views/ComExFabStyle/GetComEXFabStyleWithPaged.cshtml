
@model COM_EX_FABSTYLE

@{
    ViewData["Title"] = "Commercial Export Fabric Style";
    Layout = Layout;
}

<div class="x_panel">
    <div class="x_title">
        <h2>
            <i class="fa fa-list fa-lg text-info" aria-hidden="true"></i> Get @ViewData["Title"]
        </h2>
        <ul class="nav navbar-right panel_toolbox">
            <li class="mr-2">
                <p>
                    <a asp-action="CreateComEXFabStyle" class="btn btn-primary" accesskey="a">
                        <i class="fa fa-plus-circle" aria-hidden="true"></i> Add
                    </a>
                </p>
            </li>
            <li>
                <a class="collapse-link"><i class="fa fa-chevron-up"></i></a>
            </li>
        </ul>
        <div class="clearfix"></div>
    </div>
    <div class="x_content table-hover table-responsive">
        <table class="table" id="table">
            <thead class="thead-dark">
                <tr>
                    <th>@Html.DisplayNameFor(model => model.CREATED_AT)</th>
                    <th>@Html.DisplayNameFor(model => model.STYLENAME)</th>
                    <th>@Html.DisplayNameFor(model => model.BRAND.BRANDNAME)</th>
                    <th>@Html.DisplayNameFor(model => model.FABCODE)</th>
                    <th>@Html.DisplayNameFor(model => model.HSCODE)</th>
                    <th>@Html.DisplayNameFor(model => model.STATUS)</th>
                    <th>Actions</th>
                </tr>
            </thead>
        </table>
    </div>
</div>

@section Styles
{
    <link asp-append-version="true" href="~/lib/datatables/css/dataTables.bootstrap4.min.css" rel="stylesheet" />
}

@section Scripts
{
    <script asp-append-version="true" src="~/lib/datatables/js/jquery.dataTables.min.js"></script>
    <script asp-append-version="true" src="~/lib/datatables/js/dataTables.bootstrap4.min.js"></script>
    <script asp-append-version="true" src="~/js/comEx/GetComEXFabStyleWithPaged.js"></script>
}