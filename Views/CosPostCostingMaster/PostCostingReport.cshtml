@using DevExpress.AspNetCore
@using DenimERP.Views.CosPreCostingMaster.Reports
@using rptPostCosting = DenimERP.Views.CosPreCostingMaster.Reports.rptPostCosting
@model int

@{
    ViewData["Title"] = "Post Costing Report";
    Layout = Layout;
}
<h2>@ViewData["Title"]</h2>

@{

    @Html.DevExpress().WebDocumentViewer("WebDocumentViewer").Height("1000px").Bind(new rptPostCosting(Model))
}

@section Styles {
    <partial name="_LayoutReport" />
}

@section Scripts {
    <script asp-append-version="true" src="~/js/jquery-resizable/jquery-resizable.js"></script>
}