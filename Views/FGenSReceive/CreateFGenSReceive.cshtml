
@model FGenSReceiveViewModel

@{
    ViewData["Title"] = "General Store Receive";
}

<h2><i class="fa fa-plus-circle text-success" aria-hidden="true"></i> Create @ViewData["Title"]</h2>

<div class="container-fluid">
    <form asp-action="CreateFGenSReceive" enctype="multipart/form-data" id="form" role="form">
        <div class="panel-body row">

            <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                <h2 class="panel-title text-success"><i class="fa fa-dot-circle-o" aria-hidden="true"></i> Receive Information</h2>
                <hr />
            </div>

            <div class="col-lg-2 col-md-3 col-sm-6 col-xs-12">
                <div class="form-group">
                    <label asp-for="FGenSReceiveMaster.RCVDATE" class="control-label"></label>
                    <input asp-for="FGenSReceiveMaster.RCVDATE" class="form-control " />
                    <span asp-validation-for="FGenSReceiveMaster.RCVDATE" class="text-danger"></span>
                </div>
            </div>
            <div class="col-lg-2 col-md-3 col-sm-6 col-xs-12">
                <div class="form-group">
                    <label asp-for="FGenSReceiveMaster.RCVTID" class="control-label"></label>
                    @Html.DropDownListFor(dt => dt.FGenSReceiveMaster.RCVTID, new SelectList(Model.FBasReceiveTypesList, "RCVTID", "RCVTYPE"),
                        "Select Receive Type", htmlAttributes: new { @class = "form-control js-example-basic-single" })
                    <span asp-validation-for="FGenSReceiveMaster.RCVTID" class="text-danger"></span>
                </div>
            </div>
            <div class="col-lg-3 col-md-3 col-sm-6 col-xs-12">
                <div class="form-group">
                    <label asp-for="FGenSReceiveMaster.RCVBY" class="control-label">Received By</label>
                    @Html.DropDownListFor(dt => dt.FGenSReceiveMaster.RCVBY, new SelectList(Model.RcvEmployees, "EMPID", "FIRST_NAME", Model.RcvEmployees.Select(e => e.EMPID)), "Select Employee", new { @class = "form-control js-example-basic-single" })
                    <span asp-validation-for="FGenSReceiveMaster.RCVBY" class="text-danger"></span>
                </div>
            </div>
            <div class="col-lg-3 col-md-3 col-sm-6 col-xs-12">
                <div class="form-group">
                    <label asp-for="FGenSReceiveMaster.CHECKBY" class="control-label">Checked By</label>
                    @Html.DropDownListFor(dt => dt.FGenSReceiveMaster.CHECKBY, new SelectList(Model.CheckEmployees, "EMPID", "FIRST_NAME", Model.RcvEmployees.Select(e => e.EMPID)), "Select Employee", new { @class = "form-control js-example-basic-single" })
                    <span asp-validation-for="FGenSReceiveMaster.CHECKBY" class="text-danger"></span>
                </div>
            </div>
            <div class="col-lg-2 col-md-3 col-sm-6 col-xs-12">
                <div class="form-group">
                    <label asp-for="FGenSReceiveMaster.LC_ID" class="control-label"></label>
                    @Html.DropDownListFor(dt => dt.FGenSReceiveMaster.LC_ID, new SelectList(Model.ComImpLcdetailsesList, "TRNSID", "PINO"),
                        "Select PI No.", new { @class = "form-control js-example-basic-single" })
                    <span asp-validation-for="FGenSReceiveMaster.LC_ID" class="text-danger"></span>
                </div>
            </div>
            <div class="col-lg-2 col-md-3 col-sm-4 col-xs-12">
                <div class="form-group">
                    <label class="control-label">LC No.</label>
                    <span class="custom-form-control-bg form-control bg-e9ecef" id="lcNo">
                    </span>
                </div>
            </div>
            <div class="col-lg-2 col-md-3 col-sm-6 col-xs-12">
                <div class="form-group">
                    <label asp-for="FGenSReceiveMaster.SUPPID" class="control-label"></label>
                    @Html.DropDownListFor(dt => dt.FGenSReceiveMaster.SUPPID, new SelectList(Model.BasSupplierinfos, "SUPPID", "SUPPNAME"),
                        "Select Supplier", htmlAttributes: new { @class = "form-control js-example-basic-single" })
                    <span asp-validation-for="FGenSReceiveMaster.SUPPID" class="text-danger"></span>
                </div>
            </div>
            <div class="col-lg-2 col-md-3 col-sm-6 col-xs-12">
                <div class="form-group">
                    <label asp-for="FGenSReceiveMaster.ORIGIN" class="control-label"></label>
                    @Html.DropDownListFor(dt => dt.FGenSReceiveMaster.ORIGIN, new SelectList(Model.CountriesList, "ID", "COUNTRY_NAME"),
                        "Select Country of Origin", htmlAttributes: new { @class = "form-control js-example-basic-single" })
                    <span asp-validation-for="FGenSReceiveMaster.ORIGIN" class="text-danger"></span>
                </div>
            </div>
            <div class="col-lg-2 col-md-3 col-sm-6 col-xs-12">
                <div class="form-group">
                    <label asp-for="FGenSReceiveMaster.CHALLAN_NO" class="control-label"></label>
                    <input asp-for="FGenSReceiveMaster.CHALLAN_NO" class="form-control " />
                    <span asp-validation-for="FGenSReceiveMaster.CHALLAN_NO" class="text-danger"></span>
                </div>
            </div>
            <div class="col-lg-2 col-md-3 col-sm-6 col-xs-12">
                <div class="form-group">
                    <label asp-for="FGenSReceiveMaster.CHALLAN_DATE" class="control-label"></label>
                    <input asp-for="FGenSReceiveMaster.CHALLAN_DATE" class="form-control " />
                    <span asp-validation-for="FGenSReceiveMaster.CHALLAN_DATE" class="text-danger"></span>
                </div>
            </div>
            <div class="col-lg-2 col-md-3 col-sm-6 col-xs-12 d-none">
                <div class="form-group">
                    <label asp-for="FGenSReceiveMaster.CNFID" class="control-label"></label>
                    @Html.DropDownListFor(dt => dt.FGenSReceiveMaster.CNFID, new SelectList(Model.ComImpCnfinfosList, "CNFID", "CNFNAME"),
                        "Select CNF NO.", htmlAttributes: new { @class = "form-control js-example-basic-single" })
                    <span asp-validation-for="FGenSReceiveMaster.CNFID" class="text-danger"></span>
                </div>
            </div>
            <div class="col-lg-2 col-md-3 col-sm-6 col-xs-12 d-none">
                <div class="form-group">
                    <label asp-for="FGenSReceiveMaster.CNF_CHALLAN_NO" class="control-label"></label>
                    <input asp-for="FGenSReceiveMaster.CNF_CHALLAN_NO" class="form-control " />
                    <span asp-validation-for="FGenSReceiveMaster.CNF_CHALLAN_NO" class="text-danger"></span>
                </div>
            </div>
            <div class="col-lg-2 col-md-3 col-sm-6 col-xs-12 d-none">
                <div class="form-group">
                    <label asp-for="FGenSReceiveMaster.CNF_CHALLAN_DATE" class="control-label"></label>
                    <input asp-for="FGenSReceiveMaster.CNF_CHALLAN_DATE" class="form-control " />
                    <span asp-validation-for="FGenSReceiveMaster.CNF_CHALLAN_DATE" class="text-danger"></span>
                </div>
            </div>
            <div class="col-lg-2 col-md-3 col-sm-6 col-xs-12">
                <div class="form-group">
                    <label asp-for="FGenSReceiveMaster.TRANSPID" class="control-label"></label>
                    @Html.DropDownListFor(dt => dt.FGenSReceiveMaster.TRANSPID, new SelectList(Model.BasTransportinfos, "TRNSPID", "TRNSPNAME"),
                        "Select Transport Agent", htmlAttributes: new { @class = "form-control js-example-basic-single" })
                    <span asp-validation-for="FGenSReceiveMaster.TRANSPID" class="text-danger"></span>
                </div>
            </div>
            <div class="col-lg-2 col-md-3 col-sm-6 col-xs-12">
                <div class="form-group">
                    <label asp-for="FGenSReceiveMaster.VEHICAL_NO" class="control-label"></label>
                    <input asp-for="FGenSReceiveMaster.VEHICAL_NO" class="form-control " />
                    <span asp-validation-for="FGenSReceiveMaster.VEHICAL_NO" class="text-danger"></span>
                </div>
            </div>
            <div class="col-lg-2 col-md-3 col-sm-6 col-xs-12">
                <div class="form-group">
                    <label asp-for="FGenSReceiveMaster.GE_ID" class="control-label"></label>
                    <input asp-for="FGenSReceiveMaster.GE_ID" class="form-control " />
                    <span asp-validation-for="FGenSReceiveMaster.GE_ID" class="text-danger"></span>
                </div>
            </div>
            <div class="col-lg-2 col-md-3 col-sm-6 col-xs-12">
                <div class="form-group">
                    <label asp-for="FGenSReceiveMaster.GEDATE" class="control-label"></label>
                    <input asp-for="FGenSReceiveMaster.GEDATE" class="form-control " />
                    <span asp-validation-for="FGenSReceiveMaster.GEDATE" class="text-danger"></span>
                </div>
            </div>
            <div class="col-lg-2 col-md-3 col-sm-6 col-xs-12">
                <div class="form-group">
                    <label asp-for="FGenSReceiveMaster.REMARKS" class="control-label"></label>
                    <input asp-for="FGenSReceiveMaster.REMARKS" class="form-control " />
                    <span asp-validation-for="FGenSReceiveMaster.REMARKS" class="text-danger"></span>
                </div>
            </div>

            <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 row" id="ComImpInvoice"></div>

            <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                <h2 class="panel-title text-success"><i class="fa fa-eye" aria-hidden="true"></i> Receive Details</h2>
                <hr />
            </div>

            <div class="col-lg-4 col-md-3 col-sm-6 col-xs-12">
                <div class="form-group">
                    <label asp-for="FGenSReceiveDetails.PRODUCTID" class="control-label"></label>
                    @Html.DropDownListFor(dt => dt.FGenSReceiveDetails.PRODUCTID, new SelectList(Model.FGsProductInformationsList, "PRODID", "PRODNAME"),
                        "Select Product", htmlAttributes: new { @class = "form-control js-example-basic-single" })
                    <span asp-validation-for="FGenSReceiveDetails.PRODUCTID" class="text-danger"></span>
                </div>
            </div>
            <div class="col-lg-2 col-md-3 col-sm-4 col-xs-12">
                <div class="form-group">
                    <label class="control-label">Unit</label>
                    <span class="custom-form-control-bg form-control bg-e9ecef" id="unit">
                    </span>
                </div>
            </div>

            @*<div class="col-lg-2 col-md-3 col-sm-6 col-xs-12">
                    <div class="form-group">
                        <label asp-for="FGenSReceiveDetails.GINDID" class="control-label"></label>
                        @Html.DropDownListFor(dt => dt.FGenSReceiveDetails.GINDID, new SelectList(Model.FGenSIndentmastersList, "GINDID", "GINDNO"), "Select Indent No.", new { @class = "form-control js-example-basic-single" })
                        <span asp-validation-for="FGenSReceiveDetails.GINDID" class="text-danger"></span>
                    </div>
                </div>*@

            <div class="col-lg-2 col-md-3 col-sm-6 col-xs-12">
                <div class="form-group">
                    <label asp-for="FGenSReceiveDetails.GINDID" class="control-label"></label>
                    <select asp-for="FGenSReceiveDetails.GINDID" class="form-control js-example-basic-single">
                        <option value="">Select Indent No.</option>
                    </select>
                    <span asp-validation-for="FGenSReceiveDetails.GINDID" class="text-danger"></span>
                </div>
            </div>
            <div class="col-lg-2 col-md-3 col-sm-4 col-xs-12">
                <div class="form-group">
                    <label class="control-label">Indent Date</label>
                    <span class="custom-form-control-bg form-control" id="gindDate">
                    </span>
                </div>
            </div>
            <div class="col-lg-2 col-md-3 col-sm-6 col-xs-12">
                <div class="form-group">
                    <label asp-for="FGenSReceiveDetails.BATCHNO" class="control-label"></label>
                    <input asp-for="FGenSReceiveDetails.BATCHNO" class="form-control " />
                    <span asp-validation-for="FGenSReceiveDetails.BATCHNO" class="text-danger"></span>
                </div>
            </div>
            <div class="col-lg-2 col-md-3 col-sm-6 col-xs-12">
                <div class="form-group">
                    <label asp-for="FGenSReceiveDetails.MNGDATE" class="control-label"></label>
                    <input asp-for="FGenSReceiveDetails.MNGDATE" class="form-control " />
                    <span asp-validation-for="FGenSReceiveDetails.MNGDATE" class="text-danger"></span>
                </div>
            </div>
            <div class="col-lg-2 col-md-3 col-sm-6 col-xs-12">
                <div class="form-group">
                    <label asp-for="FGenSReceiveDetails.EXDATE" class="control-label"></label>
                    <input asp-for="FGenSReceiveDetails.EXDATE" class="form-control " />
                    <span asp-validation-for="FGenSReceiveDetails.EXDATE" class="text-danger"></span>
                </div>
            </div>
            <div class="col-lg-2 col-md-3 col-sm-6 col-xs-12">
                <div class="form-group">
                    <label asp-for="FGenSReceiveDetails.FRESH_QTY" class="control-label"></label>
                    <input asp-for="FGenSReceiveDetails.FRESH_QTY" class="form-control " />
                    <span asp-validation-for="FGenSReceiveDetails.FRESH_QTY" class="text-danger"></span>
                </div>
            </div>
            <div class="col-lg-2 col-md-3 col-sm-6 col-xs-12">
                <div class="form-group">
                    <label asp-for="FGenSReceiveDetails.REJ_QTY" class="control-label"></label>
                    <input asp-for="FGenSReceiveDetails.REJ_QTY" class="form-control " />
                    <span asp-validation-for="FGenSReceiveDetails.REJ_QTY" class="text-danger"></span>
                </div>
            </div>
            <div class="col-lg-2 col-md-3 col-sm-6 col-xs-12">
                <div class="form-group">
                    <label asp-for="FGenSReceiveDetails.ADJUSTED_WITH" class="control-label"></label>
                    @Html.DropDownListFor(dt => dt.FGenSReceiveDetails.ADJUSTED_WITH, new SelectList(Model.FGsProductInformationsList, "PRODID", "PRODNAME"),
                        "Select Product", htmlAttributes: new { @class = "form-control js-example-basic-single" })
                    <span asp-validation-for="FGenSReceiveDetails.ADJUSTED_WITH" class="text-danger"></span>
                </div>
            </div>
            <div class="col-lg-2 col-md-3 col-sm-6 col-xs-12">
                <div class="form-group">
                    <label asp-for="FGenSReceiveDetails.REMARKS" class="control-label"></label>
                    <input asp-for="FGenSReceiveDetails.REMARKS" class="form-control " />
                    <span asp-validation-for="FGenSReceiveDetails.REMARKS" class="text-danger"></span>
                </div>
            </div>

            <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                <button class="btn btn-info pull-right" type="submit">Create</button>
                <button id="addToList" class="btn btn-success pull-right" type="button">Add</button>
                <a asp-action="GetFGenSReceive" class="btn btn-dark pull-right">Back to List</a>
            </div>

            <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 table-responsive table-hover" id="fGenSReceiveDetailsTable"></div>

        </div>
    </form>
</div>

@section Styles {
    <link asp-append-version="true" href="~/css/CustomCSS/Stepper.css" rel="stylesheet" />
}

@section Scripts {
    @{await Html.RenderPartialAsync($"_ValidationScriptPartial_Except_jQuery");}
    <script asp-append-version="true" src="~/js/custom-js/Stepper.js"></script>
    <script asp-append-version="true" src="~/js/site-common.js"></script>
    <script asp-append-version="true" src="~/js/GeneralStore/Receive/CreateFGenSReceive.js"></script>
}