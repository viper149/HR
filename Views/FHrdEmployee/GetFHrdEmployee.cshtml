@model F_HRD_EMPLOYEE
<div class="x_panel">
    <div class="x_title">
        <h2><i class="fa fa-xing fa-lg text-success" aria-hidden="true"></i>@ViewData["Title"] List</h2>
        <ul class="nav navbar-right panel_toolbox">
            <li class="mr-2">
                <p>
                    <a asp-action="CreateFHrdEmployee" class="btn btn-primary"><i class="fa fa-plus-circle" aria-hidden="true"></i> Add </a>
                </p>
            </li>
            <li>
                <a class="collapse-link"><i class="fa fa-chevron-up"></i></a>
            </li>
        </ul>
        <div class="clearfix"></div>
    </div>
    <div class="x_content table-responsive table-hover" id="lcList">
        <table class="table" id="table">
            <thead>
                <tr>
                    <th>@Html.DisplayNameFor(model => model.EMPID)</th>
                    <th>@Html.DisplayNameFor(model => model.EMPNO)</th>
                    <th>@Html.DisplayNameFor(model => model.PROX_CARD)</th>
                    <th>@Html.DisplayNameFor(model => model.NAME)</th>
                    <th>@Html.DisplayNameFor(model => model.NAME_BN)</th>
                    <th>@Html.DisplayNameFor(model => model.GENDERID)</th>
                    <th>@Html.DisplayNameFor(model => model.RELIGIONID)</th>
                    <th>@Html.DisplayNameFor(model => model.BLDGRPID)</th>
                    @*<th>@Html.DisplayNameFor(model => model.COMPANYID)</th>*@
                    <th>@Html.DisplayNameFor(model => model.DESIGID)</th>
                    <th>@Html.DisplayNameFor(model => model.DEPTID)</th>
                    <th>@Html.DisplayNameFor(model => model.SECID)</th>
                    <th>@Html.DisplayNameFor(model => model.SUBSECID)</th>
                    <th>@Html.DisplayNameFor(model => model.ODID)</th>
                    <th>@Html.DisplayNameFor(model => model.NID_NO)</th>
                    <th>@Html.DisplayName("Action")</th>
                </tr>
            </thead>
        </table>
    </div>
</div>

@section Styles
{
    <link asp-append-version="true" href="~/lib/datatables/css/dataTables.bootstrap4.min.css" rel="stylesheet" />
}

@section Scripts
{
    <script asp-append-version="true" src="~/lib/datatables/js/jquery.dataTables.min.js"></script>
    <script asp-append-version="true" src="~/lib/datatables/js/dataTables.bootstrap4.min.js"></script>
    <script asp-append-version="true" src="~/js/HR/FHrdEmployee/GetFHrdEmployee.js"></script>
}