                        <span asp-validation-for="FPrInspectionCutpcsTransfer.QTY_KG" class="text-danger"></span>
                    </div>
                </div>
                <div class="col-lg-2 col-md-3 col-sm-6 col-xs-12">
                    <div class="form-group">
                        <label asp-for="FPrInspectionCutpcsTransfer.SHIFT" class="control-label"></label>
                        @Html.DropDownListFor(c => c.FPrInspectionCutpcsTransfer.SHIFT,
