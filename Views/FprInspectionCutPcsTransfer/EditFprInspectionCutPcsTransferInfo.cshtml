                            @Html.DropDownListFor(c => c.FPrInspectionCutpcsTransfer.ROLL_NO,
                            new SelectList(Model.RollList, "ROLL_ID", "ROLLNO"), "Select Roll",
                            new { @class = "form-control js-example-basic-single" })
                            <span asp-validation-for="FPrInspectionCutpcsTransfer.ROLL_NO" class="text-danger"></span>
                        </div>
                    </div>*@
                <div class="col-lg-2 col-md-3 col-sm-6 col-xs-12">
                    <div class="form-group">
                        <label asp-for="FPrInspectionCutpcsTransfer.QTY_YDS" class="control-label"></label>
                        <input asp-for="FPrInspectionCutpcsTransfer.QTY_YDS" type="" class="form-control" id="" />
                        <span asp-validation-for="FPrInspectionCutpcsTransfer.QTY_YDS" class="text-danger"></span>
                    </div>
                </div>
                <div class="col-lg-2 col-md-3 col-sm-6 col-xs-12">
                    <div class="form-group">
                        <label asp-for="FPrInspectionCutpcsTransfer.QTY_KG" class="control-label"></label>
