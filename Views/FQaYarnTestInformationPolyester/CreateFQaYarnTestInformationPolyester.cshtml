@model FQaYarnTestInformationPolyesterViewModel

@{
    ViewData["Title"] = "Yarn Test Information";
    Layout = Layout;
}

<h2><i class="fa fa-xing fa-lg text-success" aria-hidden="true"></i>Create @ViewData["Title"] <i class="fa fa-at text-info">Polyester</i></h2>
<hr />
<div class="container-fluid">
    <form id="form" role="form" asp-action="CreateFQaYarnTestInformationPolyester">
        <div class="panel panel-default setup-content" id="step-1">
            <div class="panel-body row">
                <div class="col-lg-2 col-md-3 col-sm-6 col-xs-12">
                    <div class="form-group">
                        <label asp-for="FQaYarnTestInformationPolyester.TESTDATE" class="control-label"></label>
                        <input asp-for="FQaYarnTestInformationPolyester.TESTDATE" type="date" class="form-control" id="datePick" />
                        <span asp-validation-for="FQaYarnTestInformationPolyester.TESTDATE" class="text-danger"></span>
                    </div>
                </div>
                <div class="col-lg-2 col-md-3 col-sm-6 col-xs-12">
                    <div class="form-group">
                        <label asp-for="FQaYarnTestInformationPolyester.YRCVID" class="control-label"></label>
                        @Html.DropDownListFor(dt => dt.FQaYarnTestInformationPolyester.YRCVID, new SelectList(Model.ReceiveMasterList, "YRCVID", "CHALLANNO"), "Select Challan No.", new { @class = "form-control js-example-basic-single" })
                        <span asp-validation-for="FQaYarnTestInformationPolyester.YRCVID" class="text-danger"></span>
                    </div>
                </div>
                <div class="col-lg-4 col-md-3 col-sm-6 col-xs-12">
                    <div class="form-group">
                        <label asp-for="FQaYarnTestInformationPolyester.COUNTID" class="control-label"></label>
                        @Html.DropDownListFor(dt => dt.FQaYarnTestInformationPolyester.COUNTID, new SelectList(Model.ReceiveDetailsList, "TRNSID", "PROD.COUNTNAME"), "Select Count Name", new { @class = "form-control js-example-basic-single" })
                        <span asp-validation-for="FQaYarnTestInformationPolyester.COUNTID" class="text-danger"></span>
                    </div>
                </div>

                @*<div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 row" id="AttachTo"></div*@

                <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                    <h2 class="panel-title text-success"><i class="fa fa-wrench" aria-hidden="true"></i>Details</h2>
                    <hr />
                </div>
                <div class="col-lg-2 col-md-3 col-sm-6 col-xs-12">
                    <div class="form-group">
                        <label asp-for="FQaYarnTestInformationPolyester.COLORCODE" class="control-label"></label>
                        @Html.DropDownListFor(dt => dt.FQaYarnTestInformationPolyester.COLORCODE, new SelectList(Model.Colors, "COLORCODE", "COLOR"), "Select Shade Color", new { @class = "form-control js-example-basic-single" })
                        <span asp-validation-for="FQaYarnTestInformationPolyester.COLORCODE" class="text-danger"></span>
                    </div>
                </div>
                <div class="col-lg-2 col-md-3 col-sm-6 col-xs-12">
                    <div class="form-group">
                        <label asp-for="FQaYarnTestInformationPolyester.ACTUAL_NO_FILAMENTS" class="control-label"></label>
                        <input asp-for="FQaYarnTestInformationPolyester.ACTUAL_NO_FILAMENTS" class="form-control" />
                        <span asp-validation-for="FQaYarnTestInformationPolyester.ACTUAL_NO_FILAMENTS" class="text-danger"></span>
                    </div>
                </div>
                <div class="col-lg-2 col-md-3 col-sm-6 col-xs-12">
                    <div class="form-group">
                        <label asp-for="FQaYarnTestInformationPolyester.DENIER_ACT" class="control-label"></label>
                        <input asp-for="FQaYarnTestInformationPolyester.DENIER_ACT" class="form-control" />
                        <span asp-validation-for="FQaYarnTestInformationPolyester.DENIER_ACT" class="text-danger"></span>
                    </div>
                </div>
                <div class="col-lg-2 col-md-3 col-sm-6 col-xs-12">
                    <div class="form-group">
                        <label asp-for="FQaYarnTestInformationPolyester.TENACITY_GPD" class="control-label"></label>
                        <input asp-for="FQaYarnTestInformationPolyester.TENACITY_GPD" class="form-control" />
                        <span asp-validation-for="FQaYarnTestInformationPolyester.TENACITY_GPD" class="text-danger"></span>
                    </div>
                </div>
                <div class="col-lg-2 col-md-3 col-sm-6 col-xs-12">
                    <div class="form-group">
                        <label asp-for="FQaYarnTestInformationPolyester.EXTENSION" class="control-label"></label>
                        <input asp-for="FQaYarnTestInformationPolyester.EXTENSION" class="form-control" />
                        <span asp-validation-for="FQaYarnTestInformationPolyester.EXTENSION" class="text-danger"></span>
                    </div>
                </div>
                <div class="col-lg-2 col-md-3 col-sm-6 col-xs-12">
                    <div class="form-group">
                        <label asp-for="FQaYarnTestInformationPolyester.NIP_MTR_ACT" class="control-label"></label>
                        <input asp-for="FQaYarnTestInformationPolyester.NIP_MTR_ACT" class="form-control" />
                        <span asp-validation-for="FQaYarnTestInformationPolyester.NIP_MTR_ACT" class="text-danger"></span>
                    </div>
                </div>
                <div class="col-lg-2 col-md-3 col-sm-6 col-xs-12">
                    <div class="form-group">
                        <label asp-for="FQaYarnTestInformationPolyester.NIP_RETENTION_STRENGTH" class="control-label"></label>
                        <input asp-for="FQaYarnTestInformationPolyester.NIP_RETENTION_STRENGTH" class="form-control" />
                        <span asp-validation-for="FQaYarnTestInformationPolyester.NIP_RETENTION_STRENGTH" class="text-danger"></span>
                    </div>
                </div>
                <div class="col-lg-2 col-md-3 col-sm-6 col-xs-12">
                    <div class="form-group">
                        <label asp-for="FQaYarnTestInformationPolyester.SPANDEX_AGE" class="control-label"></label>
                        <input asp-for="FQaYarnTestInformationPolyester.SPANDEX_AGE" class="form-control" />
                        <span asp-validation-for="FQaYarnTestInformationPolyester.SPANDEX_AGE" class="text-danger"></span>
                    </div>
                </div>
                <div class="col-lg-2 col-md-3 col-sm-6 col-xs-12">
                    <div class="form-group">
                        <label asp-for="FQaYarnTestInformationPolyester.NOMINAL_DRAFT" class="control-label"></label>
                        <input asp-for="FQaYarnTestInformationPolyester.NOMINAL_DRAFT" class="form-control" />
                        <span asp-validation-for="FQaYarnTestInformationPolyester.NOMINAL_DRAFT" class="text-danger"></span>
                    </div>
                </div>
                <div class="col-lg-2 col-md-3 col-sm-6 col-xs-12">
                    <div class="form-group">
                        <label asp-for="FQaYarnTestInformationPolyester.LIMHIM" class="control-label"></label>
                        <input asp-for="FQaYarnTestInformationPolyester.LIMHIM" class="form-control" />
                        <span asp-validation-for="FQaYarnTestInformationPolyester.LIMHIM" class="text-danger"></span>
                    </div>
                </div>
                <div class="col-lg-2 col-md-3 col-sm-6 col-xs-12">
                    <div class="form-group">
                        <label asp-for="FQaYarnTestInformationPolyester.PACK_WGT" class="control-label"></label>
                        <input asp-for="FQaYarnTestInformationPolyester.PACK_WGT" class="form-control" />
                        <span asp-validation-for="FQaYarnTestInformationPolyester.PACK_WGT" class="text-danger"></span>
                    </div>
                </div>

                <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
                    <div class="form-group">
                        <label asp-for="FQaYarnTestInformationPolyester.REMARKS" class="control-label"></label>
                        <textarea asp-for="FQaYarnTestInformationPolyester.REMARKS" class="form-control"></textarea>
                        <span asp-validation-for="FQaYarnTestInformationPolyester.REMARKS" class="text-danger"></span>
                    </div>

                </div>
                <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                    <button id="btnCreateYarnTestInformationPolyester" class="btn btn-success nextBtn pull-right" type="submit">Create</button>
                </div>
                <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 table-responsive table-hover">
                    @await Html.PartialAsync($"~/Views/FQaYarnTestInformationPolyester/GetFQaYarnTestInformationListPolyester.cshtml", Model.FQaYarnTestInformationPolyester)
                </div>
            </div>
        </div>
    </form>
</div>

@section Styles
{
    <link asp-append-version="true" href="~/lib/datatables/css/dataTables.bootstrap4.min.css" rel="stylesheet" />
}

@section Scripts
{
    @{await Html.RenderPartialAsync($"_ValidationScriptPartial_Except_jQuery");}
    <script asp-append-version="true" src="~/js/YarnTestInformationPolyester/CreateFQaYarnTestInformationPolyester.js"></script>
    <script asp-append-version="true" src="~/lib/datatables/js/jquery.dataTables.min.js"></script>
    <script asp-append-version="true" src="~/lib/datatables/js/dataTables.bootstrap4.min.js"></script>
    <script asp-append-version="true" src="~/js/YarnTestInformationPolyester/GetFQaYarnTestInformationPolyester.js"></script>
}