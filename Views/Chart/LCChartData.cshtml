@model LcChartDataViewModel

@{
    ViewData["Title"] = "L/C Chart Data";
}

<div class="row text-primary">
    <h2>L/C Data</h2>
</div>
<div class="row bg-light">
    <div class="form-group col-sm-6 col-md-6 col-lg-6 pt-2">
        <label asp-for="LCQty"></label>
        <span class="custom-form-control form-control text-dark ">
            @Model.LCQty
        </span>
    </div>
    <div class="form-group col-sm-6 col-md-6 col-lg-6 pt-2">
        <label asp-for="LCValue"></label>
        <span class="custom-form-control form-control text-dark ">
            @Model.LCValue
        </span>
    </div>
</div>