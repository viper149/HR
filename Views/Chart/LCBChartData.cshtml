@model LCBChartDataViewModel

@{
    ViewData["Title"] = "LCB Chart Data";
}

<div class="row text-primary">
    <h2>LCB Data</h2>
</div>
<div class="row bg-light">
    <div class="form-group col-sm-6 col-md-6 col-lg-6 pt-2">
        <label for="Ball">LCB Data:</label>

        <span class="custom-form-control form-control text-dark ">
            @Html.DisplayFor(model => model.LCBData)
        </span>
    </div>
    <div class="form-group col-sm-6 col-md-6 col-lg-6 pt-2">
        <label for="comment">Recon Data:</label>
        <span class="custom-form-control form-control text-dark">
            @Html.DisplayFor(model => model.ReconData)
        </span>
    </div>
</div>