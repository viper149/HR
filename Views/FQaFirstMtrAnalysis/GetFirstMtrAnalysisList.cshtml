@model F_QA_FIRST_MTR_ANALYSIS_M

@{
    ViewData["Title"] = "First Meter Analysis Information";
    Layout = Layout;
}

<div class="x_panel">
    <div class="x_title">
        <h2><i class="fa fa-xing fa-lg text-success" aria-hidden="true"></i>@ViewData["Title"] List</h2>
        <ul class="nav navbar-right panel_toolbox">
            <li class="mr-2">
                <p>
                    <a asp-action="CreateFQAFirstMtrAnalysis" class="btn btn-primary"><i class="fa fa-plus-circle" aria-hidden="true"></i> Add </a>
                </p>
            </li>
            <li>
                <a class="collapse-link"><i class="fa fa-chevron-up"></i></a>
            </li>
        </ul>
        <div class="clearfix"></div>
    </div>
    <div class="x_content table-responsive table-hover" id="lcList">
        <table class="table" id="table">
            <thead>
                <tr>
                    <th>@Html.DisplayNameFor(model => model.FMID)</th>
                    <th>@Html.DisplayNameFor(model => model.RPTNO)</th>
                    <th>@Html.DisplayNameFor(model => model.TRANS_DATE)</th>
                    <th>@Html.DisplayNameFor(model => model.EMPID)</th>
                    <th>@Html.DisplayNameFor(model => model.SETID)</th>
                    <th>@Html.DisplayNameFor(model => model.BEAMID)</th>
                    <th>@Html.DisplayNameFor(model => model.ACT_DENT)</th>
                    <th>@Html.DisplayNameFor(model => model.ACT_RATIO)</th>
                    <th>@Html.DisplayNameFor(model => model.ACT_REED)</th>
                    <th>@Html.DisplayNameFor(model => model.ACT_RS)</th>
                    <th>@Html.DisplayNameFor(model => model.ACT_WIDTH)</th>
                    <th>@Html.DisplayNameFor(model => model.REMARKS)</th>
                    <th>@Html.DisplayName("Action")</th>
                </tr>
            </thead>
        </table>
    </div>
</div>

@section Styles
{
    <link asp-append-version="true" href="~/lib/datatables/css/dataTables.bootstrap4.min.css" rel="stylesheet" />
}

@section Scripts
{
    <script asp-append-version="true" src="~/lib/datatables/js/jquery.dataTables.min.js"></script>
    <script asp-append-version="true" src="~/lib/datatables/js/dataTables.bootstrap4.min.js"></script>
    <script asp-append-version="true" src="~/js/FQaFirstMtrAnalysis/GetFirstMtrAnalysisList.js"></script>
}
