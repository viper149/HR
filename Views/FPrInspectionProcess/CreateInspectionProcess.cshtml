@model FPrInspectionProcessViewModel
@{
    ViewData["Title"] = "Inspection Process";
    Layout = Layout;
}
<div class="row">
    <div class="col-4">
        <h2><i class="fa fa-plus-circle fa-lg text-success" aria-hidden="true"></i>Inspection Process</h2>
        <hr />
    </div>

    <div class="col-lg-4 col-md-4 col-sm-6 col-xs-12">
        <div class="form-group mb-0">
            @Html.DropDownListFor(dt => dt.FPrInspectionDefectPoint.StyleName, new SelectList(Model.StyleList, "FABCODE", "STYLE_NAME"), "Find Style", htmlAttributes: new { @class = " form-control p-0 js-example-basic-single" })
        </div>
    </div>
    <div class="col-lg-4 col-md-4 col-sm-6 col-xs-12">
        <div class="form-group mb-0">
            <input asp-for="FPrInspectionDefectPoint.FindDate" class=" form-control height17" />
            <span asp-validation-for="FPrInspectionDefectPoint.FindDate" class="text-danger"></span>
        </div>
    </div>
</div>


<div class="container-fluid">
    <form id="form" role="form" asp-action="CreateInspectionProcess">
        <div class="panel panel-default">
            <div class="panel-body row m-0 p-0">
                <div class="col-12 row m-0 p-0">
                    <div class="col-6 row m-0 p-0">
                        <div class="col-12">
                            <h2 class="panel-title text-success">Order Information</h2>
                            <hr class="m-0" />
                        </div>
                        <div class="col-6 pl-0 pr-0">
                            <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                                @Html.HiddenFor(c => c.FPrInspectionProcessMaster.CREATED_AT)
                                @Html.HiddenFor(c => c.FPrInspectionProcessMaster.UPDATED_AT)
                                @Html.HiddenFor(c => c.FPrInspectionProcessMaster.FABCODE)
