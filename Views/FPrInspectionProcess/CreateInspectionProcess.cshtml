                                    <div class="input-group-prepend" style="max-width: 25%; min-width: 25%;">
                                        <div class="input-group-text p-0 custom-form-control-bg-label"><label asp-for="FPrInspectionProcessMaster.INSPDATE" class="control-label labelSm"></label></div>
                                    </div>
                                    <input asp-for="FPrInspectionProcessMaster.INSPDATE" class="form-control height17" readonly="readonly" />
                                    <span asp-validation-for="FPrInspectionProcessMaster.INSPDATE" class="text-danger"></span>
                                </div>
                            </div>

                            <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 d-none">
