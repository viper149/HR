
@model ACC_LOCAL_DOMASTER

@{
    ViewData["Title"] = "Account DO (Local)";
}

@{
    <div class="x_panel">
        <div class="x_title">
            <h2><i class="fa fa-list fa-lg text-info" aria-hidden="true"></i> Get @ViewData["Title"]</h2>
            <ul class="nav navbar-right panel_toolbox">
                <li class="mr-2">
                    <p>
                        <a asp-action="CreateAccDoMaster" class="btn btn-primary" accesskey="a"><i class="fa fa-plus-circle" aria-hidden="true"></i> Add</a>
                    </p>
                </li>
                <li>
                    <a class="collapse-link"><i class="fa fa-chevron-up"></i></a>
                </li>
            </ul>
            <div class="clearfix"></div>
        </div>
        <div class="x_content table-responsive table-hover">
            <table class="table" id="table">
                <thead class="thead-dark">
                    <tr>
                        <th>@Html.DisplayNameFor(model => model.DONO)</th>
                        <th>@Html.DisplayNameFor(model => model.DODATE)</th>
                        <th>@Html.DisplayNameFor(model => model.SCNO)</th>
                        <th>@Html.DisplayNameFor(model => model.AUDITBY)</th>
                        <th>@Html.DisplayNameFor(model => model.AUDITON)</th>
                        <th>@Html.DisplayNameFor(model => model.COMMENTS)</th>
                        <th>@Html.DisplayNameFor(model => model.REMARKS)</th>
                        <th>@Html.DisplayName("Actions")</th>
                    </tr>
                </thead>
            </table>
        </div>
    </div>
}

@section Styles
{
    <link asp-append-version="true" href="~/lib/datatables/css/dataTables.bootstrap4.min.css" rel="stylesheet" />
}

@section Scripts
{
    <script asp-append-version="true" src="~/lib/datatables/js/jquery.dataTables.min.js"></script>
    <script asp-append-version="true" src="~/lib/datatables/js/dataTables.bootstrap4.min.js"></script>
    <script asp-append-version="true" src="~/js/AccLocalDoMaster/GetAccLocalDoMaster.js"></script>
}