                <th scope="col">@Html.DisplayName("Actions")</th>
            </tr>
        </thead>
        <tbody id="myTable">
            @for (var i = 0; i < Model.FPrInspectionFabricDDetailsList.Count; i++)
            {
            <tr>
                <td>
                    @Html.HiddenFor(clc => clc.FPrInspectionFabricDDetailsList[i].TRNSID)
                    @Html.HiddenFor(clc => clc.FPrInspectionFabricDDetailsList[i].DID)
                    @Html.HiddenFor(clc => clc.FPrInspectionFabricDDetailsList[i].FABCODE)
                    @Model.FPrInspectionFabricDDetailsList[i].FABCODENavigation?.STYLE_NAME
                </td>
                <td>
                    @Html.HiddenFor(clc => clc.FPrInspectionFabricDDetailsList[i].SO_NO)
                    @Model.FPrInspectionFabricDDetailsList[i].SO_NONavigation?.SO_NO
                </td>
                <td>
                    @Html.HiddenFor(clc => clc.FPrInspectionFabricDDetailsList[i].PO_NO)
                    @Model.FPrInspectionFabricDDetailsList[i].SO_NONavigation?.PIMASTER?.PINO
                </td>
                <td>
                    @Html.HiddenFor(clc => clc.FPrInspectionFabricDDetailsList[i].ROLL_ID)
                    @Model.FPrInspectionFabricDDetailsList[i].ROLL_?.ROLLNO
                </td>
                <td>
                    @Html.HiddenFor(clc => clc.FPrInspectionFabricDDetailsList[i].ROLL_ID)
                    @Model.FPrInspectionFabricDDetailsList[i].ROLL_?.SHIFT
                </td>
                <td>
                    @Html.HiddenFor(clc => clc.FPrInspectionFabricDDetailsList[i].QTY_YARDS)
                    @Model.FPrInspectionFabricDDetailsList[i].ROLL_?.LENGTH_YDS
                </td>
                <td>
                    @Model.FPrInspectionFabricDDetailsList[i].ROLL_?.FAB_GRADE
                </td>
                @*<td>
            @Model.FPrInspectionFabricDDetailsList[i].ROLL_?.OPT1
        </td>
        <td>
            @Html.HiddenFor(clc => clc.FPrInspectionFabricDDetailsList[i].LOCATION)
            @Model.FPrInspectionFabricDDetailsList[i].LOCATIONNavigation?.LOCATION
        </td>*@
                <td>
                    @Html.HiddenFor(clc => clc.FPrInspectionFabricDDetailsList[i].REMARKS)
                    @Model.FPrInspectionFabricDDetailsList[i].ROLL_?.REMARKS
                </td>
                @*<td>
            <i class="fa fa-trash fa-2x-custom text-danger" onclick="RemoveFabricFromList(@i)" aria-hidden="true"></i>
        </td>*@
                <td>
                    <i class="fa fa-trash fa-lg text-danger" onclick="RemoveDispatch(@i)" aria-hidden="false"></i>
                </td>
                

            </tr>
            }
        </tbody>
    </table>
}



