
@model F_PR_INSPECTION_FABRIC_D_DETAILS

@{
    ViewData["Title"] = "Inspection Roll QC Approve";
    Layout = Layout;
}

@{
    <div class="x_panel">
        <div class="x_title">
            <h2>
                <i class="fa fa-list fa-lg text-info" aria-hidden="true"></i>@ViewData["Title"]
            </h2>
            <ul class="nav navbar-right panel_toolbox">
                <li class="mr-4">
                    <p>
                        <a asp-action="CreateRollReceive" class="btn btn-primary"><i class="fa fa-plus-circle" aria-hidden="true"></i> Add</a>
                    </p>
                </li>
                <li>
                    <a class="collapse-link"><i class="fa fa-chevron-up"></i></a>
                </li>
            </ul>
            <div class="clearfix"></div>
        </div>
        <div class="x_content">
            <table class="table" id="table">
                <thead>
                    <tr>
                        <th>
                            @Html.DisplayNameFor(model => model.D.DDATE)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.FABCODE)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.SO_NO)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.PO_NO)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.ROLL_ID)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.QTY_YARDS)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.REMARKS)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.IS_QC_APPROVE)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.IS_QC_REJECT)
                        </th>
                    </tr>
                </thead>
            </table>
        </div>
    </div>

}

@section Styles
{
    <link asp-append-version="true" href="~/lib/datatables/css/dataTables.bootstrap4.min.css" rel="stylesheet" />
}

@section Scripts
{
    <script asp-append-version="true" src="~/lib/datatables/js/jquery.dataTables.min.js"></script>
    <script asp-append-version="true" src="~/lib/datatables/js/dataTables.bootstrap4.min.js"></script>
    <script asp-append-version="true" src="~/js/FPrInspectionFabricDispatch/GetRollReceiveListForQc.js"></script>
}