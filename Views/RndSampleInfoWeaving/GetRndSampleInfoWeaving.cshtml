
@model RND_SAMPLE_INFO_WEAVING

@{
    ViewData["Title"] = "Rnd Sample Info Weaving";
    Layout = Layout;
}

<div class="x_panel">
    <div class="x_title">
        <h2>
            <i class="fa fa-flask fa-lg text-info"></i> Rnd Sample Info <i class="fa fa-at text-info">Weaving</i>
        </h2>
        <ul class="nav navbar-right panel_toolbox">
            <li class="mr-2">
                <p>
                    <a asp-action="CreateRndSampleInfoWeaving" class="btn btn-primary"><i class="fa fa-plus-circle" aria-hidden="true"></i> Add </a>
                </p>
            </li>
            <li>
                <a class="collapse-link"><i class="fa fa-chevron-up"></i></a>
            </li>
        </ul>
        <div class="clearfix"></div>
    </div>
    <div class="x_content table-responsive table-hover" id="lcList">
        <table class="table" id="table">
            <thead>
                <tr>
                    <th>
                        @Html.DisplayNameFor(model => model.FABCODE)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.TOTAL_ENDS)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.REED_DENT)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.GR_EPI)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.GR_PPI)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.REMARKS)
                    </th>
                    <th>
                        @Html.DisplayName("Action")
                    </th>
                </tr>
            </thead>
        </table>
    </div>
</div>

@section Styles
{
    <link href="~/lib/datatables/css/dataTables.bootstrap4.min.css" rel="stylesheet" asp-append-version="true" />
}

@section Scripts
{
    <script src="~/lib/datatables/js/jquery.dataTables.min.js" asp-append-version="true"></script>
    <script src="~/lib/datatables/js/dataTables.bootstrap4.min.js" asp-append-version="true"></script>
    <script src="~/js/RndSampleInfoWeaving/GetRndSampleInfoWeaving.js" asp-append-version="true"></script>
}