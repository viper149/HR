        <div class="row">
            <div class="col-md-12">
                <div class="row">
                    <div class="col-lg-12">
                        <div class="m-b-md">
                            <h2 class="text-danger">@Html.DisplayNameFor(e => e.RndSampleInfoDyeing.SDID) : [ @Model.RndSampleInfoDyeing.SDID ]</h2>
                            <hr />
                        </div>
                    </div>
                </div>
                <div class="row details">
                    <div class="col-2">
                        <div class="form-group">
                            <strong>
                                <label asp-for="RndSampleInfoDyeing.SIDATE" class="control-label"></label>
                            </strong>
                            <span class="custom-form-control form-control bg-e9ecef">
                                @Html.DisplayFor(model => model.RndSampleInfoDyeing.SIDATE)
                            </span>
                        </div>
                    </div>
                    <div class="col-2">
                        <div class="form-group">
                            <strong>
                                <label asp-for="RndSampleInfoDyeing.SDRF.SDRF_NO" class="control-label"></label>
                            </strong>
                            <span class="custom-form-control form-control bg-e9ecef">
                                @Html.DisplayFor(model => model.RndSampleInfoDyeing.SDRF.SDRF_NO)
                            </span>
                        </div>
                    </div>
                    <div class="col-2">
                        <div class="form-group">
                            <strong>
                                <label asp-for="RndSampleInfoDyeing.DYEINGCODE" class="control-label"></label>
