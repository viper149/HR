                            <strong>
                                <label asp-for="RndSampleInfoDyeing.RNDPERSON" class="control-label"></label>
                            </strong>
                            <span class="custom-form-control form-control bg-e9ecef">
                                @Html.DisplayFor(model => model.RndSampleInfoDyeing.RNDPERSON)
                            </span>
                        </div>
                    </div>
                    <div class="col-2">
                        <div class="form-group">
                            <strong>
                                <label asp-for="RndSampleInfoDyeing.ENDS_ROPE" class="control-label"></label>
