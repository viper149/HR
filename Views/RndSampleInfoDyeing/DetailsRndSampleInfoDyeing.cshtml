                    </div>
                    <div class="col-2">
                        <div class="form-group">
                            <strong>
                                <label asp-for="RndSampleInfoDyeing.LENGTH_MTR" class="control-label"></label>
                            </strong>
                            <span class="custom-form-control form-control bg-e9ecef">
                                @Html.DisplayFor(model => model.RndSampleInfoDyeing.LENGTH_MTR)
                            </span>
                        </div>
                    </div>
                    <div class="col-2">
