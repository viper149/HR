
@model DetailsRndSampleInfoDyeingViewModel

@{
    ViewData["Title"] = "Details RnD Sample Info Dyeing";
    Layout = Layout;
}

<h2><i class="fa fa-info-circle fa-lg text-success" aria-hidden="true"></i> RnD Sample Information Dyeing</h2>

